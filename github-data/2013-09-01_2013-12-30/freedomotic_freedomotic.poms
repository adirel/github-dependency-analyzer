                                            <type>directory</type>
                                            <mapper>
                                                <type>perm</type>
                                                <prefix>/usr/share/freedomotic/lib</prefix>
                                                <user>freedomotic</user>
                                                <group>freedomotic</group>
                                                <filemode>664</filemode>
                                            </mapper>
                                        </data>
                                        <data>
                                            <src>framework/freedomotic-core/config</src>
                                            <type>directory</type>
                                            <excludes>**/*~,serial/**</excludes>
                                            <mapper>
                                                <type>perm</type>
