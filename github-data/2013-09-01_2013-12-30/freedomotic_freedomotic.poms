                                            <mapper>
                                                <type>perm</type>
                                                <prefix>/usr/share/freedomotic/plugins</prefix>
                                                <user>freedomotic</user>
                                                <group>freedomotic</group>
                                                <filemode>664</filemode>
                                                <dirmode>775</dirmode>
                                            </mapper>
                                        </data>
                                        <data>
