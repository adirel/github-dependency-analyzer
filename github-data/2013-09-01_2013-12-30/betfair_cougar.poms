    </parent>

    <artifactId>baseline-app</artifactId>
    <packaging>jar</packaging>
    <name>baseline-app</name>

    <build>
        <plugins>
            <plugin>
                <groupId>com.betfair.cougar</groupId>
                <artifactId>cougar-codegen-plugin</artifactId>
                <version>${project.version}</version>
                <executions>
                    <execution>
                        <id>baseline</id>
                        <goals>
                            <goal>process</goal>
                        </goals>
                        <configuration>
                            <services>
                                <service>
                                    <serviceName>BaselineService</serviceName>
                                </service>
                            </services>
                            <iddAsResource>true</iddAsResource>
                            <!-- run as both a client and a server -->
                            <client>true</client>
                            <server>true</server>
                        </configuration>
                    </execution>
                    <execution>
                        <id>non</id>
                        <goals>
                            <goal>process</goal>
                        </goals>
                        <configuration>
                            <services>
                                <service>
                                    <serviceName>NonService</serviceName>
                                </service>
                            </services>
                            <iddAsResource>true</iddAsResource>
                            <!-- client only -->
                            <client>true</client>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
            </resource>
            <resource>
                <directory>target/generated-sources/java</directory>
                <includes>
                    <include>**/jaxb.index</include>
                </includes>
            </resource>
            <resource>
                <directory>target/generated-resources</directory>
                <includes>
                    <include>**/*.wsdl</include>
                    <include>**/*.xsd</include>
                    <include>**/*.idd</include>
                </includes>
            </resource>
        </resources>
    </build>
    <dependencies>
        <dependency>
            <groupId>com.betfair.cougar</groupId>
            <artifactId>baseline-idd</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
