                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <services>
                        <service>
                            <serviceName>SocketPerfTesting</serviceName>
                        </service>
                    </services>
                    <iddAsResource>true</iddAsResource>
                    <!-- run as both a client and a server -->
                    <client>false</client>
                    <server>true</server>
                </configuration>
            </plugin>
        </plugins>
        <resources>
