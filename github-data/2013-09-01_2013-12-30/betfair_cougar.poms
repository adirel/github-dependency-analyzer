	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.betfair.cougar</groupId>
		<artifactId>baseline</artifactId>
		<version>3.1-SNAPSHOT</version>
	</parent>
	<artifactId>baseline-launch</artifactId>
	<packaging>jar</packaging>

	<name>baseline-launcher</name>
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-assembly-plugin</artifactId>
					<version>2.2-beta-3</version>
				</plugin>
			</plugins>
		</pluginManagement>

		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>1.1.1</version>
				<configuration>
					<mainClass>com.betfair.cougar.core.impl.Main</mainClass>
					<classpathScope>runtime</classpathScope>
                    <systemProperties>
                        <systemProperty>
                            <key>user.timezone</key>
                            <value>UTC</value>
                        </systemProperty>
                        <systemProperty>
                            <key>system.timezone</key>
                            <value>UTC</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.log.CONSOLE.level</key>
                            <value>INFO</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.log.echoToStdout</key>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.maxmind.useDefault</key>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.fault.detailed</key>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jmx.html.keystore.filepath</key>
                            <value>classpath:cougar_server_cert.jks</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jmx.html.keystore.password</key>
                            <value>ENC(iTXZuGkVHRJoOrDQ548Ua8TZw7998U+h)</value> <!-- "password" encrypted with algorithm=PBEWithMD5AndDES and password=baseline -->
                        </systemProperty>
                        <systemProperty>
                            <key>jmx.html.keystore.certpassword</key>
                            <value>ENC(iTXZuGkVHRJoOrDQ548Ua8TZw7998U+h)</value> <!-- "password" encrypted with algorithm=PBEWithMD5AndDES and password=baseline -->
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.healthStateRepeatsBeforeUpdate</key>
                            <value>1</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.healthMonitorStrategy</key>
                            <value>active</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.healthMonitorInterval</key>
                            <value>2</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.logLevel</key>
                            <value>TRANSPORT</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.client.socket.logLevel</key>
                            <value>TRANSPORT</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.client.socket.clientExecutor.processorThreads</key>
                            <value>2</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.log.SESSION-LOG.format</key>
                            <value>%d{HH:mm:ss.SSS}: %t %m%n</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.client.socket.ssl.supportsTls</key>
                            <value>false</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.client.socket.ssl.requiresTls</key>
                            <value>false</value>
                        </systemProperty>
                        <!-- override default so we can test insecure clients -->
                        <systemProperty>
                            <key>cougar.socket.ssl.requiresTls</key>
                            <value>false</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.ssl.supportsTls</key>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.ssl.wantClientAuth</key>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.ssl.keystore</key>
                            <value>classpath:cougar_server_cert.jks</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.ssl.keystorePassword</key>
                            <value>ENC(iTXZuGkVHRJoOrDQ548Ua8TZw7998U+h)</value> <!-- "password" encrypted with algorithm=PBEWithMD5AndDES and password=baseline -->
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.ssl.truststore</key>
                            <value>classpath:cougar_client_ca.jks</value>
                        </systemProperty>
                        <systemProperty>
                            <key>cougar.socket.ssl.truststorePassword</key>
                            <value>ENC(iTXZuGkVHRJoOrDQ548Ua8TZw7998U+h)</value> <!-- "password" encrypted with algorithm=PBEWithMD5AndDES and password=baseline -->
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.port</key>
                            <value>8443</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.wantClientAuth</key>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.certAlias</key>
                            <value>selfsigned</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.keystore</key>
                            <value>classpath:cougar_server_cert.jks</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.keyPassword</key>
                            <value>ENC(iTXZuGkVHRJoOrDQ548Ua8TZw7998U+h)</value> <!-- "password" encrypted with algorithm=PBEWithMD5AndDES and password=baseline -->
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.truststore</key>
                            <value>classpath:cougar_client_ca.jks</value>
                        </systemProperty>
                        <systemProperty>
                            <key>jetty.https.trustPassword</key>
