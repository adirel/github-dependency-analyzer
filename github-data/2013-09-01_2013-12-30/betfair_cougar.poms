                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <version>2.2.1</version>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <!-- Download source jars when mvn eclipse plugin is used and fix the version of the plugin -->
            <plugin>
                <artifactId>maven-eclipse-plugin</artifactId>
                <version>2.5.1</version>
                <configuration>
                    <downloadSources>true</downloadSources>
                </configuration>
            </plugin>

            <!-- Disable the default test execution and replace it with unit test and integration test executions
                        bound to the test and integration-test phases of the build, respectively.
                    By default compile all the tests and run them i.e. ${skipTests} is set to false in this pom.
                    This additional property is a workaround for http://jira.codehaus.org/browse/SUREFIRE-319
                    Some projects override the skipTests property and set it to true to skip (compile but not run)
                    their tests by default. This is because their tests compile but do not pass yet.
                    Set -DskipTests=false at the command line to run all tests. Set -DskipTests to skip all tests.
                    Similarly set testFailureIgnore with a property or at the command line (-DtestFailureIgnore=false) to override. -->
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.14.1</version>
                <configuration>
                    <skipTests>true</skipTests>
                </configuration>
                <executions>
                    <execution>
                        <id>unit-test</id>
                        <goals>
                            <goal>test</goal>
                        </goals>
                        <phase>test</phase>
                        <configuration>
                            <testSourceDirectory>src/test</testSourceDirectory>
                            <includes>
                                <include>**/*Test.java</include>
                            </includes>
                            <excludes>
                                <exclude>**/*$*</exclude>
                                <exclude>**/integrationtest/**/*.java</exclude>
                                <exclude>**/performancetest/**/*.java</exclude>
                                <exclude>**/functionaltest/**/*.java</exclude>
                            </excludes>
                            <skipTests>${skipTests}</skipTests>
                            <testFailureIgnore>false</testFailureIgnore>
                        </configuration>
                    </execution>
                    <execution>
                        <id>integration-test</id>
                        <goals>
                            <goal>test</goal>
                        </goals>
                        <phase>integration-test</phase>
                        <configuration>
                            <testSourceDirectory>src/test</testSourceDirectory>
                            <includes>
                                <include>**/integrationtest/**/*Test.java</include>
                                <include>**/integrationtest/**/*TestCase.java</include>
                                <include>**/integrationtest/**/Test*.java</include>
                            </includes>
                            <skipTests>${skipTests}</skipTests>
                            <testFailureIgnore>false</testFailureIgnore>
                            <argLine>-Xms256m -Xmx768m</argLine>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

        </plugins>
    </build>
