		<dependency>
			<groupId>com.yammer.metrics</groupId>
			<artifactId>metrics-core</artifactId>
			<version>2.2.0</version>
			<scope>test</scope>
		</dependency>

		<!-- native arch/os stats -->
		<!-- <dependency> -->
		<!-- <groupId>org.fusesource</groupId> -->
		<!-- <artifactId>sigar</artifactId> -->
		<!-- <version>1.6.4</version> -->
		<!-- <scope>test</scope> -->
