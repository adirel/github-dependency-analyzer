                    <groupId>org.datanucleus</groupId>
                    <artifactId>datanucleus-enhancer</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>${rootArtifactId}-fixture</artifactId>
            <exclusions>
                <exclusion>
                    <!-- so don't pick up transitive dependency to asm 4.0.0 -->
                    <groupId>org.datanucleus</groupId>
