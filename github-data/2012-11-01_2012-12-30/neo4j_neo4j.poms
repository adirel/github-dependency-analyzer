              </generateToc>
              <postProcess>
                <copy todir="${project.build.directory}/docbkx/webhelp/images" overwrite="true">
                  <fileset dir="${docs.tools}/main/resources/images"/>
                </copy>
                <copy todir="${project.build.directory}/docbkx/webhelp/images" overwrite="true">
                  <fileset dir="${project.build.directory}/src/images"/>
