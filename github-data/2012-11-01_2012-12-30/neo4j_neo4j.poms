          <sectionAutolabel>1</sectionAutolabel>
          <sectionAutolabelMaxDepth>1</sectionAutolabelMaxDepth>
          <appendixAutolabel>true</appendixAutolabel>
          <sourceDirectory>${project.build.directory}</sourceDirectory>
