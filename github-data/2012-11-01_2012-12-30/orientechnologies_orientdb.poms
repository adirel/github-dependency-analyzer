        </snapshotRepository>
    </distributionManagement>

    <!-- required to include Tinkerpop SNAPSHOT dependencies -->
    <repositories>
        <repository>
            <id>sonatype-nexus-snapshots</id>
            <name>Sonatype Nexus Snapshots</name>
