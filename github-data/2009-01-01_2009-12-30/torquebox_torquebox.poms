<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <parent>
    <groupId>org.torquebox</groupId>
    <artifactId>torquebox-build</artifactId>
    <version>3.1.1-SNAPSHOT</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <modelVersion>4.0.0</modelVersion>

  <groupId>rubygems</groupId>
  <artifactId>torquebox-server</artifactId>
  <packaging>java-gem</packaging>
  <name>TorqueBox Server Gem</name>
  <description />
  <url>http://torquebox.org/</url>

  <dependencies>

    <dependency>
      <groupId>org.torquebox</groupId>
      <artifactId>torquebox-dist</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
    </dependency>

    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>torquebox</artifactId>
      <version>${project.version}</version>
      <type>gem</type>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rspec</artifactId>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rake</artifactId>
      <type>gem</type>
      <scope>test</scope>
    </dependency>

  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>rspec-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>run-specs</id>
            <phase>test</phase>
            <goals>
              <goal>test</goal>
            </goals>
            <configuration>
              <fork>true</fork>
              <jrubyJvmArgs>
                -Dlog4j.configuration=test-log4j.xml
              </jrubyJvmArgs>
            </configuration>
          </execution>
        </executions>
        </plugin>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>gem-maven-plugin</artifactId>
        <configuration>
          <jrubyJvmArgs>-Xmx1024m</jrubyJvmArgs>
          <extraFiles>jboss/**/*,share/**/*</extraFiles>
        </configuration>
      </plugin>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>jruby-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>copy-jboss-and-share</id>
            <phase>process-sources</phase>
            <goals>
              <goal>jruby</goal>
            </goals>
            <configuration>
              <script>
                require 'fileutils'
                def cp( src, dest )
                  puts %Q(copying #{src} to #{dest})
                  FileUtils.mkdir_p( dest )
                  FileUtils.cp_r( src, dest )
                end
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss),
                    %Q(${basedir}/target/torquebox-server) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/share),
                    %Q(${basedir}/target/torquebox-server) )
              </script>
            </configuration>
          </execution>
          <execution>
            <id>copy-gem</id>
            <inherited>true</inherited>
            <phase>install</phase>
            <goals>
              <goal>jruby</goal>
            </goals>
            <configuration>
              <script>
                basedir = '${project.basedir}'.gsub('\\', '/')
                load %Q(#{basedir}/../assembly/lib/assembly_tool.rb)
                ext = '.gem'
                ext = ( ( 'java-gem' == '${project.packaging}' ) ? '-java.gem' : '.gem' )
                safe_version = '${project.version}'.gsub(/-SNAPSHOT/, '')
                AssemblyTool.copy_gem_to_repo( %Q(#{basedir}/target/${project.artifactId}-#{safe_version}#{ext}) )
              </script>
              <jrubyJvmArgs>-Xmx1024m</jrubyJvmArgs>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

</project>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <parent>
    <groupId>org.torquebox</groupId>
    <artifactId>torquebox-docs-getting-started</artifactId>
    <version>3.1.1-SNAPSHOT</version>
    <relativePath>../</relativePath>
  </parent>

  <modelVersion>4.0.0</modelVersion>

  <groupId>org.torquebox</groupId>
  <artifactId>torquebox-docs-getting-started-en_US</artifactId>
  <packaging>jdocbook</packaging>
  <name>TorqueBox Getting Started Guide (${translation})</name>
  <description>Getting Started Guide for TorqueBox (${translation})</description>
  
  <properties>
    <translation>en_US</translation>
    <translation.dash>en-US</translation.dash>
  </properties>

  <build>
    <plugins>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <executions>
          <execution>
            <id>create-bundles</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/html.xml</descriptor>
              </descriptors>
            </configuration>
          </execution>
          <execution>
            <id>create-epub-stage1</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <attach>false</attach>
              <descriptors>
                <descriptor>src/main/assembly/epub.xml</descriptor>
              </descriptors>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-artifacts</id>
            <phase>package</phase>
            <goals>
              <goal>attach-artifact</goal>
            </goals>
            <configuration>
