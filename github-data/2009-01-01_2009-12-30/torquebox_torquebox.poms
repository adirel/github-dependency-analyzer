  <dependencies>
    <dependency>
      <groupId>org.torquebox</groupId>
      <artifactId>torquebox-build-assembly</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>json</artifactId>
      <type>gem</type>
      <version>${version.json}</version>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <executions>
          <execution>
            <id>dist-bin</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal> 
            </goals>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/dist-bin.xml</descriptor>
              </descriptors>
            </configuration>
          </execution>
          <execution>
            <id>dist-modules</id>
            <phase>package</phase> 
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/dist-modules.xml</descriptor>
              </descriptors>
            </configuration>
          </execution>
          <execution>
            <id>dist-rubygem-repo</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/dist-rubygem-repo.xml</descriptor>
              </descriptors>
            </configuration>
          </execution>
          <execution>
            <id>dist-src</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/dist-src.xml</descriptor>
              </descriptors>
            </configuration>
          </execution>          
        </executions>
      </plugin>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>jruby-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>generate-build-metadata</id>
            <phase>package</phase>
            <goals>
              <goal>jruby</goal>
            </goals>
            <configuration>
              <file>bin/generate_build_metadata.rb</file>
            </configuration>
          </execution>
          <execution>
            <id>generate-sha1</id>
            <phase>package</phase>
            <goals>
              <goal>jruby</goal>
            </goals>
            <configuration>
              <file>bin/generate_sha1.rb</file>
              <args>target/*.{zip,json}</args>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

</project>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <parent>
    <groupId>org.torquebox</groupId>
    <artifactId>torquebox-build</artifactId>
    <version>3.1.1-SNAPSHOT</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <modelVersion>4.0.0</modelVersion>

  <groupId>org.torquebox</groupId>
  <artifactId>torquebox-eap-overlay</artifactId>
  <packaging>pom</packaging>
  <name>TorqueBox Build EAP Overlay</name>

  <dependencies>
    <dependency>
      <groupId>org.torquebox</groupId>
      <artifactId>torquebox-build-assembly</artifactId>
      <type>pom</type>
      <version>${project.version}</version>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>jruby-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>copy-jruby-and-modules</id>
            <phase>process-sources</phase>
            <goals>
              <goal>jruby</goal>
            </goals>
            <configuration>
              <script>
                require 'fileutils'
                def cp( src, dest )
                  puts %Q(copying #{src} to #{dest})
                  FileUtils.mkdir_p( dest )
                  FileUtils.cp_r( src, dest )
                end
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jruby),
                    %Q(${basedir}/target/torquebox-eap-overlay) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss/modules/system/layers/polyglot),
                    %Q(${basedir}/target/torquebox-eap-overlay/modules/system/layers) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss/modules/system/layers/torquebox),
                    %Q(${basedir}/target/torquebox-eap-overlay/modules/system/layers) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss/modules/layers.conf),
                    %Q(${basedir}/target/torquebox-eap-overlay/modules) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss/standalone/configuration/torquebox-full.xml),
                    %Q(${basedir}/target/torquebox-eap-overlay/standalone/configuration) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss/standalone/configuration/torquebox-full-ha.xml),
                    %Q(${basedir}/target/torquebox-eap-overlay/standalone/configuration) )
                cp( %Q(${basedir}/../assembly/target/stage/torquebox/jboss/domain/configuration/torquebox-full.xml),
                    %Q(${basedir}/target/torquebox-eap-overlay/domain/configuration) )
              </script>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

</project>
