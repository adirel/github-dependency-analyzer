                  <descriptorRefs combine.self="override" />
                  <descriptors>
                    <descriptor>src/main/assemblies/scm-source-release.xml</descriptor>
                  </descriptors>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>

