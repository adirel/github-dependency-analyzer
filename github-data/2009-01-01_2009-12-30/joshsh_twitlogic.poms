            <version>3.1.3</version>
        </dependency>

        <!-- Communication -->
        <dependency>
            <groupId>oauth.signpost</groupId>
            <artifactId>signpost-commonshttp4</artifactId>
        </dependency>

        <!-- Sesame RDF -->
