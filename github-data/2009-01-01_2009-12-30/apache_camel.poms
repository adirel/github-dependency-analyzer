  <properties>
    <camel.osgi.export.pkg>
      org.apache.camel.component.sql.*;${camel.osgi.version},
      org.apache.camel.processor.aggregate.jdbc.*;${camel.osgi.version},
      org.apache.camel.processor.idempotent.jdbc.*;${camel.osgi.version}
