    <description>An example using Camel CXF (code first) with Apache Tomcat</description>
    <packaging>war</packaging>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>tomcat-maven-plugin</artifactId>
                <configuration>
                    <server>myTomcat</server>
                    <url>${tomcat.url}</url>
