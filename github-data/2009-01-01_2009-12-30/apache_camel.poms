                            <testSourceRoot>${basedir}/target/generated/src/test/java</testSourceRoot>
                            <defaultOptions>
                                <frontEnd>jaxws21</frontEnd>
                            </defaultOptions>
                            <wsdlOptions>
                                <wsdlOption>
                                    <wsdl>
                                        ${basedir}/src/test/resources/org/apache/camel/dataformat/soap/CustomerService.wsdl
