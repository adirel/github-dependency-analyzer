    <version>3.0-SNAPSHOT</version>
    <name>grizzly-modules</name>
    <profiles>
        <profile>
            <id>coverage</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>merge-cobertura-datafiles</id>
                                <phase>process-test-classes</phase>
                                <configuration>
                                    <tasks>
                                        <taskdef classpathref="maven.runtime.classpath" resource="tasks.properties" />
                                        <mkdir dir="target/cobertura" />
                                        <cobertura-merge datafile="target/cobertura/cobertura.ser">
                                            <fileset dir=".">
                                                <include name="**/cobertura.ser" />
                                            </fileset>
                                        </cobertura-merge>
                                    </tasks>
                                </configuration>
                                <goals>
                                    <!--suppress MavenModelInspection -->
                                    <goal>run</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>cobertura-maven-plugin</artifactId>
                        <!--suppress MavenModelInspection -->
                        <version>${cobertura.version}</version>
                        <configuration>
                            <formats>
                                <format>xml</format>
                                <format>html</format>
                            </formats>
                        </configuration>
                        <executions>
                            <execution>
                                <goals>
                                    <!--suppress MavenModelInspection -->
                                    <goal>clean</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
    <modules>
        <module>grizzly</module>
        <module>compression</module>
        <module>http</module>
        <module>http-server</module>
        <module>http-servlet</module>
        <module>comet</module>
        <module>websockets</module>
        <module>portunif</module>
        <module>http-ajp</module>
        <module>spdy</module>
        <module>monitoring</module>
        <module>bundles</module>
    </modules>
</project>
