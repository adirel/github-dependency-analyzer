    <build>
        <resources>
            <resource>
                <directory>${basedir}/target/src/main/resources</directory>
            </resource>
            <resource>
                <directory>${basedir}/src/main/resources</directory>
            </resource>
            <resource>
                <directory>${gen-src-dir}</directory>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
