                            org.glassfish.grizzly.*;version=${grizzly.version},
                            *
                        </Import-Package>
                    </instructions>
                    <unpackBundle>true</unpackBundle>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <properties>
        <gen-src-dir>${basedir}/target/src/main/java</gen-src-dir>
    </properties>

</project>
