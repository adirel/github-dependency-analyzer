            <scope>provided</scope>
            <version>${servlet.api.version}</version>
        </dependency>
        <dependency>
            <groupId>org.glassfish.tyrus</groupId>
            <artifactId>tyrus-server</artifactId>
        </dependency>
    </dependencies>
</project>
