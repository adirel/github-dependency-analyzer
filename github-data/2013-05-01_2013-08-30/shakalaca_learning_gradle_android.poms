						<phase>site</phase>
						<goals>
							<goal>site</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<message>Creating site for ${project.version}.</message>
					<outputDirectory>website</outputDirectory>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
