<macrodef name="compile-with-picard" >
	<attribute name="name"/>
	<attribute name="java"/>
	<attribute name="main"/>
	<sequential>
	   <mkdir dir="tmp"/>
	   <mkdir dir="dist"/>
	   <mkdir dir="${generated.dir}"/>
	   <echo>if the next javac statement fails, check that the 'build.properties' has been properly configured. See https://github.com/lindenb/jvarkit/wiki/Compilation </echo>
	   <javac destdir="tmp" includeAntRuntime="false" debug="true">
	   		<src path="src/main/java"/>
   			<src path="${generated.dir}"/>

	   		<classpath refid="picard.lib"/>
	   		<include name="@{java}"/>
	   		<include name="**/package-info.java"/>
	   </javac>
	   
	 <loadfile
	      property="git.master.hash"
	      srcFile=".git/refs/heads/master"
	 	  failonerror="false"
	 	  >
	 	  	<filterchain>
	 	        <striplinebreaks/>
			</filterchain>
	    </loadfile>
		
	   <pathconvert property="manifest_@{name}" pathsep=" " >
			 <path refid="picard.lib"/>
	   </pathconvert>  
	   
	<tstamp>
    	<format property="TODAY" pattern="yyyy-MM-dd HH:mm:ss" />
	</tstamp>
	   
	   <jar jarfile="dist/@{name}.jar">
		     <fileset dir="tmp" />
		     <manifest>
		       <attribute name="Main-Class" value="@{main}"/>
		        <attribute name="Class-Path" value="${manifest_@{name}}"/>
		    	<attribute name="Git-Hash" value="${git.master.hash}"/>
		    	<attribute name="Compile-Date" value="${TODAY}"/>
		     </manifest>
	 	</jar>
	   
	   <echo file="dist/@{name}">#!/bin/bash
PREFIX=$(dirname $0)
java -Xmx500m -jar $PREFIX/@{name}.jar $*
</echo>
	   
	   <chmod file="dist/@{name}" perm="ugo+rx"/>
		
		<echo><![CDATA[<h4>Compilation</h4>
```bash
ant ]]>@{name}<![CDATA[
```
<h4>Options</h4>
]]></echo>
		<document-command
			jar="dist/@{name}.jar"
			mainclass="@{main}"
			/>
	    
		<echo><![CDATA[<h4>Example</h4>
```bash
java -jar dist/]]>@{name}<![CDATA[.jar [options]
```
]]></echo>		

		<delete dir="tmp"/>
	</sequential>
</macrodef>



<macrodef name="compile-picard-cmd">
	<attribute name="name"/>
	<attribute name="java"/>
	<attribute name="main"/>
	<attribute name="libraryid" default="picard.lib"/>
	<sequential>
	  
	   <mkdir dir="tmp"/>
	   <mkdir dir="dist"/>
	   <mkdir dir="${generated.dir}"/>
	   <mkdir dir="tmp/META-INF"/>
	   
	   <copy file="src/main/resources/messages/messages.properties" todir="tmp"/>
	   
	   <echo>if the next javac statement fails, check that the 'build.properties' has been properly configured. See https://github.com/lindenb/jvarkit/wiki/Compilation</echo>
	   <javac destdir="tmp" includeAntRuntime="false" debug="true">
	   		
	   		<src path="src/main/java"/>
   			<src path="${generated.dir}"/>

	   		<classpath refid="@{libraryid}"/>
	   		<include name="@{java}"/>
	   		<include name="**/package-info.java"/>
	   </javac>
	   
	 <loadfile
	      property="git.master.hash"
	      srcFile=".git/refs/heads/master"
	 	  failonerror="false"
	 	  >
	 	  	<filterchain>
	 	        <striplinebreaks/>
			</filterchain>
	    </loadfile>
		
	   <pathconvert property="manifest_@{name}" pathsep=" " >
			 <path refid="@{libraryid}"/>
	   </pathconvert>  

	   <pathconvert property="manifest_@{name}_for_exe" >
			 <path refid="@{libraryid}"/>
	   </pathconvert>  

	   
	<tstamp>
    	<format property="TODAY" pattern="yyyy-MM-dd HH:mm:ss" />
	</tstamp>
	   
	   <jar jarfile="dist/@{name}.jar">
		     <fileset dir="tmp" />
		     <manifest>
		       <attribute name="Main-Class" value="@{main}"/>
		        <attribute name="Class-Path" value="${manifest_@{name}}"/>
		    	<attribute name="Git-Hash" value="${git.master.hash}"/>
		    	<attribute name="Compile-Date" value="${TODAY}"/>
		     </manifest>
	 	</jar>
	   
	   
	   	   <echo file="dist/@{name}">#!/bin/bash
PREFIX=$(dirname $0)
java -Xmx500m -cp ${manifest_@{name}_for_exe}:$PREFIX/@{name}.jar @{main} $*
</echo>
	   
	   <chmod file="dist/@{name}" perm="ugo+rx"/>
	   	   
		<delete dir="tmp"/>
		
		
	</sequential>
</macrodef>


