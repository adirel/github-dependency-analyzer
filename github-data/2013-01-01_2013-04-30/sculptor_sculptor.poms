	<artifactId>sculptor-framework-main</artifactId>
	<name>Sculptor :: Runtime Framework :: Main</name>

	<properties>
		<!-- We have to build against Hibernate 3.5 because Hibernate3 with JPA1 is still supported -->
		<!-- FIXME using this runtime library with Hibernate4 will break in GenericEnumUserType,
			 JpaHibFindByConditionStatAccessImpl and NumericLikeExpression due to changed Hibernate API
			 (see https://github.com/sculptor/sculptor/issues/79) -->
		<hibernate-version>3.5.5-Final</hibernate-version>
	</properties>
 
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<executions>
					<execution>
						<id>sculptor-framework-publicapi</id>
						<goals>
							<goal>jar</goal>
						</goals>
						<phase>package</phase>
						<configuration>
							<classifier>publicapi</classifier>
							<includes>
								<include>**/event/Event.class</include>
								<include>**/domain/*.class</include>
								<include>**/errorhandling/*Exception.class</include>
								<include>**/errorhandling/ServiceContext.class</include>
								<include>**/errorhandling/annotation/ApplicationException.class</include>
								<include>**/validation/validator/*.class</include>
								<include>**/util/EqualsHelper.class</include>
							</includes>
							<excludes>
								<exclude>**/domain/Property.class</exclude>
								<exclude>**/domain/PropertiesCollection.class</exclude>
								<exclude>**/domain/LeafProperty.class</exclude>
							</excludes>
						</configuration>
