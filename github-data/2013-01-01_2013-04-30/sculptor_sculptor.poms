<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.sculptorgenerator</groupId>
		<artifactId>sculptor-eclipse</artifactId>
		<version>3.0.3</version>
	</parent>
	<groupId>org.sculptorgenerator.eclipse</groupId>
	<artifactId>eclipse-repository</artifactId>
	<packaging>eclipse-repository</packaging>
	<name>Sculptor :: Eclipse :: p2 Repository</name>

	<build>
		<plugins>
			<!-- Deploying the p2 repository to a GitHub repository branch as described 
				in http://stackoverflow.com/questions/14013644/hosting-a-maven-repository-on-github/ -->
			<plugin>
				<groupId>com.github.github</groupId>
				<artifactId>site-maven-plugin</artifactId>
				<configuration>
					<message>Sculptor Eclipse bundles ${project.version}</message>
					<outputDirectory>${project.build.directory}/repository</outputDirectory>
					<repositoryName>sculptor.github.io</repositoryName>
					<branch>refs/heads/master</branch>
					<path>updates</path>
					<merge>true</merge>
					<noJekyll>false</noJekyll>
					<message>creates Sculptor Elipse update site ${project.version}</message>
				</configuration>
			</plugin>
		</plugins>
	</build>

</project>
<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.sculptorgenerator</groupId>
		<artifactId>sculptor-parent</artifactId>
		<version>3.0.3</version>
	</parent>
	<artifactId>sculptor-eclipse</artifactId>
	<packaging>pom</packaging>
	<name>Sculptor :: Eclipse</name>

	<modules>
		<module>org.sculptor.dsl</module>
		<module>org.sculptor.dsl.ui</module>
		<module>org.sculptor.dsl.tests</module>
		<module>org.sculptor.dsl.feature</module>
	</modules>

	<profiles>
		<profile>
			<id>release-finish</id>
			<activation>
				<property>
					<name>performRelease</name>
					<value>true</value>
				</property>
			</activation>
			<modules>
				<module>eclipse-repository</module>
			</modules>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-source-plugin</artifactId>
						<configuration>
							<skip>true</skip>
						</configuration>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-javadoc-plugin</artifactId>
						<configuration>
							<skip>true</skip>
						</configuration>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-gpg-plugin</artifactId>
						<configuration>
							<skip>true</skip>
						</configuration>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-deploy-plugin</artifactId>
						<configuration>
							<skip>true</skip>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>

	<repositories>
		<!-- Repository to resolve the Eclipse release train dependencies -->
		<repository>
			<id>p2.eclipse</id>
			<url>${p2-repository-eclipse}</url>
			<layout>p2</layout>
		</repository>
		<!-- Repository to resolve the XText dependencies -->
		<repository>
			<id>p2.eclipse.xtext</id>
			<url>${p2-repository-eclipse-xtext}</url>
			<layout>p2</layout>
		</repository>
		<!-- Repository to resolve the xtext-utils dependencies -->
		<repository>
			<id>p2.xtext-utils</id>
			<url>${p2-repository-xtext-utils}</url>
			<layout>p2</layout>
		</repository>
	</repositories>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.eclipse.xtend</groupId>
					<artifactId>xtend-maven-plugin</artifactId>
					<configuration>
						<outputDirectory>${basedir}/xtend-gen</outputDirectory>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>

		<plugins>
			<!-- Activate Tycho -->
			<plugin>
				<groupId>org.eclipse.tycho</groupId>
				<artifactId>tycho-maven-plugin</artifactId>
				<extensions>true</extensions>
			</plugin>
			<plugin>
				<groupId>org.eclipse.tycho</groupId>
				<artifactId>target-platform-configuration</artifactId>
				<!-- Sculptors p2 repository must contain only Sculptors Eclipse plugins -->
				<configuration>
					<filters>
						<filter>
							<type>java-package</type>
							<id>org</id>
							<restrictTo>
								<type>p2-installable-unit</type>
								<id>org.sculptor.dsl.feature.feature.group</id>
							</restrictTo>
						</filter>
					</filters>
				</configuration>
			</plugin>
		</plugins>
	</build>

</project>
