                                <goals>
                                    <goal>process</goal>
                                </goals>
                            </execution>
                        </executions>
                        <configuration>
                            <xml>src/main/resources/wuic.xml</xml>
                            <relocateTransformedXml>true</relocateTransformedXml>
                            <output>${build.finalName}</output>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

