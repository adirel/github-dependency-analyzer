<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.google.appengine.tck</groupId>
        <artifactId>appengine-tck</artifactId>
        <version>1.0.0-SNAPSHOT</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <artifactId>appengine-tck-tests</artifactId>
    <packaging>pom</packaging>
    <name>GAE TCK Tests</name>
    <url>https://developers.google.com/appengine/</url>
    <description>Google AppEngine Test Compatibility Kit</description>

    <modules>
        <!-- APIs -->
        <module>appengine-tck-appidentity</module>
        <module>appengine-tck-blobstore</module>
        <module>appengine-tck-capability</module>
        <module>appengine-tck-channel</module>
        <module>appengine-tck-datastore</module>
        <module>appengine-tck-example</module>
        <module>appengine-tck-images</module>
        <module>appengine-tck-logservice</module>
        <module>appengine-tck-mail</module>
        <module>appengine-tck-memcache</module>
        <module>appengine-tck-modules</module>
        <module>appengine-tck-oauth</module>
        <module>appengine-tck-prospectivesearch</module>
        <module>appengine-tck-search</module>
        <module>appengine-tck-taskqueue</module>
        <module>appengine-tck-urlfetch</module>
        <module>appengine-tck-users</module>
        <module>appengine-tck-xmpp</module>
    </modules>

    <properties>
        <ignore.gae.jar>false</ignore.gae.jar>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.google.appengine</groupId>
            <artifactId>appengine-api-1.0-sdk</artifactId>
        </dependency>

        <dependency>
            <groupId>com.google.appengine.tck</groupId>
            <artifactId>appengine-tck-base</artifactId>
        </dependency>

        <dependency>
            <groupId>org.jboss.arquillian.junit</groupId>
            <artifactId>arquillian-junit-container</artifactId>
            <scope>test</scope>
        </dependency>

        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <profiles>
        <!-- Default, compile tests, but not run -->
        <profile>
            <id>default</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <skipTests>true</skipTests>
            </properties>

            <build>
                <plugins>
                    <plugin>
                        <groupId>com.google.appengine.tck</groupId>
                        <artifactId>appengine-tck-coverage</artifactId>
                        <version>${project.version}</version>
                        <executions>
                            <execution>
                                <id>api-coverage</id>
                                <goals>
                                    <goal>coverage</goal>
                                </goals>
                                <phase>process-test-classes</phase>
                                <configuration>
                                    <module>tests</module>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>

            <dependencies>
                <dependency>
                    <groupId>com.google.appengine.tck</groupId>
                    <artifactId>appengine-tck-coverage</artifactId>
                </dependency>
            </dependencies>
        </profile>

        <!-- GAE -->
        <profile>
            <id>sdk</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>sdk</name>
                </property>
            </activation>

            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-surefire-plugin</artifactId>
                        <configuration>
                            <redirectTestOutputToFile>true</redirectTestOutputToFile>
                            <includes>
                                <include>com/google/appengine/tck/**/*Test.java</include>
                            </includes>
                            <systemProperties>
                                <appengine.test.datastore.default_high_rep_job_policy_unapplied_job_pct>20</appengine.test.datastore.default_high_rep_job_policy_unapplied_job_pct>
                            </systemProperties>
                        </configuration>
                    </plugin>
                </plugins>
            </build>

            <dependencies>
                <dependency>
                    <groupId>com.google.appengine.tck</groupId>
                    <artifactId>sdk-env</artifactId>
                </dependency>

                <dependency>
                    <groupId>org.jboss.arquillian.container</groupId>
                    <artifactId>arquillian-gae-local</artifactId>
                    <scope>test</scope>
                </dependency>
            </dependencies>
        </profile>

        <!-- GAE remote - AppSpot -->
        <profile>
            <id>appspot</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>appspot</name>
                </property>
            </activation>
            <build>
                <plugins>
                    <plugin>
