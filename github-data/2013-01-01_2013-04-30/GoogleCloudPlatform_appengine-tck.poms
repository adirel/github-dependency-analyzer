                            </includes>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
            <dependencies>
                <dependency>
                    <groupId>com.google.appengine.tck</groupId>
                    <artifactId>appengine-tck-multisuite</artifactId>
                    <scope>test</scope>
                </dependency>
            </dependencies>
        </profile>
