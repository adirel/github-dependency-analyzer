    <name>Myrrix Ltd</name>
    <url>http://myrrix.com/</url>
  </organization>

  <developers>
    <developer>
      <id>srowen</id>
      <name>Sean Owen</name>
    </developer>
  </developers>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <skipTests>true</skipTests>
    <commons.compress.version>1.6</commons.compress.version>
    <commons.math.version>3.2</commons.math.version>
    <google.guava.version>14.0.1</google.guava.version>
    <jewelcli.version>0.8.3</jewelcli.version>
    <junit.version>4.11</junit.version>
    <mahout.version>0.8</mahout.version>
    <slf4j.version>1.7.5</slf4j.version>
    <tomcat.version>7.0.47</tomcat.version>
  </properties>
  
  <prerequisites>
    <maven>3.0</maven>
  </prerequisites>
  
