  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
          <failOnError>true</failOnError>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.mycila.maven-license-plugin</groupId>
        <artifactId>maven-license-plugin</artifactId>
        <configuration>
          <header>header.txt</header>
          <mapping>
            <xreg>XML_STYLE</xreg>
            <psml>XML_STYLE</psml>
          </mapping>
          <includes>
            <include>**/src/**/*.java</include>
            <include>**/src/**/*.vm</include>
            <include>**/src/**/*.html</include>
            <include>**/src/**/*.xml</include>
            <include>**/src/**/*.xreg</include>
            <include>**/src/**/*.psml</include>
            <include>**/src/**/*.js</include>
            <include>**/src/**/*.css</include>
            <include>**/src/**/*.css</include>
            <include>**/src/**/*.jsp</include>
            <include>**/src/**/*.xsl</include>
            <include>**/src/**/*.properties</include>
            <include>**/*.sql</include>
          </includes>
          <excludes>
            <exclude>**/src/main/java/org/apache/**/*.*</exclude>
            <exclude>**/src/main/resources/org/apache/**/*.*</exclude>
            <exclude>**/src/test/java/org/apache/**/*.*</exclude>
            <exclude>**/src/test/resources/org/apache/**/*.*</exclude>
            <exclude>**/src/main/webapp/javascript/dijit/**/*.*</exclude>
            <exclude>**/src/main/webapp/javascript/dojo/**/*.*</exclude>
            <exclude>**/src/main/webapp/javascript/dojox/**/*.*</exclude>
            <exclude>**/src/main/webapp/javascript/aipo.js</exclude>
            <exclude>**/*.min.js</exclude>
            <exclude>**/*.ycomp.js</exclude>
            <exclude>**/*-all.js</exclude>
            <exclude>**/*.min.css</exclude>
          </excludes>
          <encoding>UTF-8</encoding>
          <strictCheck>true</strictCheck>
          <skipExistingHeaders>false</skipExistingHeaders>
          <properties>
            <year>2011</year>
          </properties>
          <headerDefinitions>
            <headerDefinition>header-definition.xml</headerDefinition>
          </headerDefinitions>
        </configuration>
      </plugin>
    </plugins>
  </build>

