				<artifactId>maven-assembly-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<appendAssemblyId>true</appendAssemblyId>
					<descriptors>
						<descriptor>src/main/assembly/assembly.xml</descriptor>
					</descriptors>
					<archive>
						<manifest>
							<mainClass>io.selendroid.SelendroidLauncher</mainClass>
							<UseUniqueVersions>true</UseUniqueVersions>
						</manifest>
						<manifestEntries>
							<url>${project.parent.url}</url>
							<version>${project.version}</version>
						</manifestEntries>
					</archive>
				</configuration>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>attached</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<resources>
			<resource>
				<directory>../</directory>
				<filtering>false</filtering>
				<includes>
					<include>LICENSE</include>
					<include>README.md</include>
				</includes>
			</resource>
			<resource>
				<directory>../selendroid-server/</directory>
				<filtering>false</filtering>
				<includes>
					<include>AndroidManifest.xml</include>
				</includes>
			</resource>
			<resource>
				<directory>../selendroid-server/target/</directory>
				<filtering>false</filtering>
				<includes>
					<include>selendroid-server-${project.version}.apk</include>
				</includes>
				<targetPath>prebuild/</targetPath>
			</resource>
			<resource>
				<directory>../android-driver/target/</directory>
				<filtering>false</filtering>
				<includes>
					<include>android-driver-app-${project.version}.apk</include>
				</includes>
				<targetPath>prebuild/</targetPath>
			</resource>
		</resources>
	</build>

</project>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>io.selendroid</groupId>
		<artifactId>selendroid</artifactId>
		<version>0.10.0-SNAPSHOT</version>
	</parent>
	<artifactId>selendroid-test-app</artifactId>
	<packaging>apk</packaging>
	<properties>
		<skipTests>true</skipTests>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<dependencies>
