                            <artifactId>cglib</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
            </dependencies>
        </profile>
        <!-- =============================================================== -->
        <!-- GEOPLATFORM TRACKING -->
        <!-- =============================================================== -->
        <profile>
            <id>tracking</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>full</name>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geoplatform-tracking-api</artifactId>
                </dependency>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geoplatform-tracking-impl</artifactId>
                </dependency>
            </dependencies>
        </profile>
        <!-- =============================================================== -->
        <!-- GEOPLATFORM GEONETWORK FINDER -->
        <!-- =============================================================== -->
        <profile>
            <id>finder</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>full</name>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geonetwork-catalog-api</artifactId>
                </dependency>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geonetwork-catalog-impl</artifactId>
                </dependency>
            </dependencies>
        </profile>
        <!-- =============================================================== -->
        <!-- GEOPLATFORM CSW CATALOG FINDER -->
        <!-- =============================================================== -->
        <profile>
            <id>csw</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>full</name>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>csw-catalog-api</artifactId>
                </dependency>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>csw-catalog-impl</artifactId>
                </dependency>
            </dependencies>
        </profile>
        
        <!--================================================================= -->
        <!-- Run cobertura on the sources of geoplatform-ws-core-impl -->
        <!--================================================================= -->
        <profile>
            <id>cob</id>
            <!-- <activation><activeByDefault /></activation> -->
            <dependencies>
                <!-- TODO Is it indispensable? -->
                <!-- Include referenced classes -->
                <dependency>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>cobertura-maven-plugin</artifactId>
                    <version>2.5.1</version>
                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <!-- Extract geoplatform-ws-impl classes (already instrumented) -->
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-dependency-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>unpack_ws_impl</id>
                                <phase>process-classes</phase>
                                <goals>
                                    <goal>unpack</goal>
                                </goals>
                                <configuration>
                                    <artifactItems>
                                        <artifactItem>
                                            <groupId>org.geosdi</groupId>
                                            <artifactId>geoplatform-ws-core-impl</artifactId>
                                            <!-- <classifier>flowdata</classifier> -->
                                            <type>jar</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/classes</outputDirectory>
                                            <includes>**/*.class</includes>
                                        </artifactItem>
                                    </artifactItems>
                                    <overWriteReleases>true</overWriteReleases>
                                    <overWriteSnapshots>true</overWriteSnapshots>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
    <build>
        <defaultGoal>jetty:run</defaultGoal>
        <!-- Name of the generated WAR file -->
        <finalName>geoplatform-service</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-failsafe-plugin</artifactId>
                <version>2.6</version>
                <executions>
                    <execution>
                        <id>failsafe-integration-test</id>
                        <phase>integration-test</phase>
                        <goals>
                            <goal>integration-test</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>failsafe-verify</id>
                        <phase>verify</phase>
                        <goals>
                            <goal>verify</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
            
            <plugin>
                <groupId>org.mortbay.jetty</groupId>
                <artifactId>jetty-maven-plugin</artifactId>
                <version>${jetty-version}</version>
                <configuration>
                    <scanIntervalSeconds>0</scanIntervalSeconds>
                    <stopKey>foo</stopKey>
                    <stopPort>9999</stopPort>
                    <webApp>
                        <contextPath>/geoplatform-service</contextPath>
                    </webApp>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>

