                    <exclusions>
                        <exclusion>
                            <groupId>cglib</groupId>
                            <artifactId>cglib</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
            </dependencies>
        </profile>
        
        <!-- =============================================================== -->
        <!-- GEOPLATFORM WFS -->
        <!-- =============================================================== -->
        <profile>
            <id>wfs</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>full</name>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geoplatform-wfs-api</artifactId>
                    <exclusions>
                        <exclusion>
                            <groupId>cglib</groupId>
                            <artifactId>cglib</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geoplatform-wfs-impl</artifactId>
                    <exclusions>
                        <exclusion>
                            <groupId>cglib</groupId>
                            <artifactId>cglib</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
            </dependencies>
        </profile>
        
        <!-- =============================================================== -->
        <!-- GEOPLATFORM PUBLISHER -->
        <!-- =============================================================== -->
        <profile>
            <id>publisher</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>full</name>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>org.geosdi</groupId>
                    <artifactId>geoplatform-publisher-api</artifactId>
                    <exclusions>
                        <exclusion>
                            <groupId>cglib</groupId>
                            <artifactId>cglib</artifactId>
                        </exclusion>
                    </exclusions>
