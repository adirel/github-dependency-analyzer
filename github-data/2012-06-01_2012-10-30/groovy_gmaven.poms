    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.codehaus.gmaven.it.gmaven-plugin.execute</groupId>
        <artifactId>common</artifactId>
        <version>@project.version@</version>
        <relativePath>../common.xml</relativePath>
    </parent>

    <artifactId>execute-props-6</artifactId>

    <description>
        Verify that setting properties works.
    </description>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.gmaven</groupId>
                <artifactId>gmaven-plugin</artifactId>
                <executions>
                    <!-- Make sure put/get works -->
                    <execution>
                        <id>1</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                project.properties['foo'] = 'bar'
                                assert project.properties['foo'] == 'bar'
                            </source>
                        </configuration>
                    </execution>

                    <execution>
                        <id>2</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                assert project.properties['foo'] == 'bar'
                            </source>
                        </configuration>
                    </execution>

                    <!-- Make sure set|get property works -->
                    <execution>
                        <id>3</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                project.properties.setProperty('foo1', 'bar')
                                assert project.properties.getProperty('foo1') == 'bar'
                            </source>
                        </configuration>
                    </execution>

                    <execution>
                        <id>4</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                assert project.properties.getProperty('foo1') == 'bar'
                            </source>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <!-- Make sure another plugin sees the same changes. -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-antrun-plugin</artifactId>
                <executions>
                    <execution>
                        <id>1</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>run</goal>
                        </goals>
                        <configuration>
                            <tasks>
                                <delete dir="${project.build.directory}"/>
                                <mkdir dir="${project.build.directory}"/>
                                <echo>${foo}${foo1}</echo>
                                <echo file="${project.build.directory}/props.txt">${foo}${foo1}</echo>
                            </tasks>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>

