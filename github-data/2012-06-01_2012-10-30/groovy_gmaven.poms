<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.codehaus.gmaven.it.gmaven-plugin</groupId>
        <artifactId>common</artifactId>
        <version>@project.version@</version>
        <relativePath>../../common.xml</relativePath>
    </parent>

    <artifactId>project-sources-1</artifactId>

    <description>
        Test that Groovy compile source root and test compile source root are added to project.
    </description>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.gmaven</groupId>
                <artifactId>gmaven-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <goals>
                            <goal>addSource</goal>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                println "Project sources:  ${project.getCompileSourceRoots()}"
                                println "Project test sources:  ${project.getTestCompileSourceRoots()}"
                                assert project.getCompileSourceRoots().size() == 2
                                assert project.getCompileSourceRoots()[0].endsWith(File.separator + "src" + File.separator + "main" + File.separator + "java")
                                assert project.getCompileSourceRoots()[1].endsWith(File.separator + "src" + File.separator + "main" + File.separator + "groovy")
                                assert project.getTestCompileSourceRoots().size() == 2
                                assert project.getTestCompileSourceRoots()[0].endsWith(File.separator + "src" + File.separator + "test" + File.separator + "java")
                                assert project.getTestCompileSourceRoots()[1].endsWith(File.separator + "src" + File.separator + "test" + File.separator + "groovy")
                            </source>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>
