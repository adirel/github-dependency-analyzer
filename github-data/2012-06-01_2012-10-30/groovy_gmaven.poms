        <version>@project.version@</version>
        <relativePath>../common.xml</relativePath>
    </parent>

    <artifactId>execute-defaults</artifactId>

    <description>
        Test access to the default variables injected for groovy:execute.
    </description>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.gmaven</groupId>
                <artifactId>gmaven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>1</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                println log
                                println log?.class
                                assert log instanceof org.slf4j.Logger
                                log.info("Hello")

                                println project
                                println project?.class
                                assert project instanceof org.apache.maven.project.MavenProject
                                assert pom
                                assert pom == project

                                println settings
                                println settings?.class
                                assert settings instanceof org.apache.maven.settings.Settings

                                println session
                                println session?.class
                                assert session instanceof org.apache.maven.execution.MavenSession

                                println ant
                                println ant?.class
                                assert ant instanceof groovy.util.AntBuilder

                                ant.echo(level: "info", "Hello")
                                ant.echo(level: "debug", "Goodbye")

                                assert fail
                                // can't test fail though...
                            </source>
                        </configuration>
                    </execution>

                    <execution>
                        <id>2</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
                                // Make sure we can clobber them if needed
                                log = false
                                assert !log

                                pom = false
                                assert !pom

                                project = false
                                assert !project

                                ant = false
                                assert !ant

                                fail = false
                                assert !fail
                            </source>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>

