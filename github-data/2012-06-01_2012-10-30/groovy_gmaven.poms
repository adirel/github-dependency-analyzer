    <parent>
        <groupId>org.codehaus.gmaven.it.gmaven-plugin.execute</groupId>
        <artifactId>common</artifactId>
        <version>@project.version@</version>
        <relativePath>../common.xml</relativePath>
    </parent>

    <artifactId>execute-props-3</artifactId>

    <description>
        Verify that custom properties are picked up per execution.
    </description>

    <properties>
        <prop1>${prop2}</prop1>
        <prop2>${prop3}</prop2>
        <!-- prop3 is defined below in an execution's properties -->
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.gmaven</groupId>
                <artifactId>gmaven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>1</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <properties>
                                <prop3>hello</prop3>
                            </properties>

                            <source>
                                println "----"
                                project.properties.each {
                                    println it
                                }
                                println "----"
                                println ""

                                assert project.properties['prop1'] == 'hello'
                            </source>
                        </configuration>
                    </execution>

                    <execution>
                        <id>2</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <properties>
                                <prop3>${project}</prop3>
                            </properties>

                            <source>
                                import org.apache.maven.project.MavenProject

                                println "prop1: " + project.properties['prop1']
                                println "prop3: " + project.properties['prop3']

                                assert project.properties['prop1'] instanceof MavenProject
                                assert project.properties['prop3'] instanceof MavenProject

                                def proj = project.properties['prop1']
                                assert proj != null
                                assert proj.artifactId == project.artifactId
                            </source>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>

