            <artifactId>provisionr-cloudstack</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.axemblr.provisionr</groupId>
            <artifactId>provisionr-console</artifactId>
            <scope>provided</scope>
        </dependency>
