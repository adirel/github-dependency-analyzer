    <name>Axemblr Provisionr :: Core</name>
    <description>Includes Activiti Engine configuration and common building blocks</description>
    <artifactId>provisionr-core</artifactId>
    <packaging>bundle</packaging>

    <properties>
        <osgi.import>*</osgi.import>
        <osgi.export>com.axemblr.provisionr.core*</osgi.export>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>org.osgi.core</artifactId>
        </dependency>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
        </dependency>
