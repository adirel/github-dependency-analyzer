	<name>Revmob adapter for AdMob Mediation</name>
	<description>Revmob adapter for AdMob Mediation</description>

	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
		</dependency>
		<dependency>
			<groupId>com.google.android</groupId>
			<artifactId>android</artifactId>
		</dependency>
		<dependency>
			<groupId>com.google.ads.admob</groupId>
			<artifactId>admob</artifactId>
		</dependency>
		<dependency>
			<groupId>com.revmob</groupId>
			<artifactId>revmob-ads</artifactId>
			<version>6.6.0</version>
		</dependency>
	</dependencies>

</project>
