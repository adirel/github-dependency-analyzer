				<directory>${project.basedir}/target/aix_ppc</directory>
			</build>
			<properties>
				<swt.version>org.eclipse.swt.gtk.aix.ppc</swt.version>
			</properties>
		</profile>
		<profile>
			<id>aix_ppx64</id>
			<build>
				<directory>${project.basedir}/target/aix_ppc64</directory>
			</build>
			<properties>
				<swt.version>org.eclipse.swt.gtk.aix.ppc64</swt.version>
			</properties>
		</profile>
		<profile>
			<id>hpux64</id>
			<build>
				<directory>${project.basedir}/target/hpux64</directory>
			</build>
			<properties>
				<swt.version>org.eclipse.swt.gtk.hpux.ia64_32</swt.version>
			</properties>
		</profile>
		<profile>
			<id>linux_ppc64</id>
			<build>
				<directory>${project.basedir}/target/linux_ppc64</directory>
			</build>
			<properties>
