			<version>4.31</version>
		</dependency>
		<dependency>
			<groupId>org.xerial</groupId>
			<artifactId>sqlite-jdbc</artifactId>
			<version>3.6.20</version>
		</dependency>
		<dependency>
			<groupId>org.quartz-scheduler</groupId>
			<artifactId>quartz</artifactId>
			<version>2.1.3</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.17</version>
		</dependency>
		<dependency>
			<groupId>commons-daemon</groupId>
			<artifactId>commons-daemon</artifactId>
			<version>1.0.10</version>
		</dependency>
		<dependency>
			<groupId>org.eclipse.swt</groupId>
			<artifactId>${swt.version}</artifactId>
			<version>4.3</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-log4j12</artifactId>
			<version>1.6.1</version>
		</dependency>
		<dependency>
			<groupId>org.eclipse.nebula</groupId>
			<artifactId>nebula</artifactId>
			<version>0.5.3</version>
		</dependency>
		<dependency>
			<groupId>info.ajaxplorer.eclipse</groupId>
			<artifactId>org.eclipse.equinox.common</artifactId>
			<version>3.6</version>
		</dependency>
		<dependency>
			<groupId>info.ajaxplorer.eclipse</groupId>
			<artifactId>org.eclipse.core.commands</artifactId>
			<version>3.6.1</version>
		</dependency>
		<dependency>
			<groupId>info.ajaxplorer.eclipse</groupId>
			<artifactId>org.eclipse.jface</artifactId>
			<version>3.8</version>
		</dependency>
		<dependency>
			<groupId>info.ajaxplorer.eclipse</groupId>
			<artifactId>org.eclipse.ui.forms</artifactId>
			<version>3.5</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.mapdb</groupId>
			<artifactId>mapdb</artifactId>
			<version>0.9.6</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>com.scireum</groupId>
			<artifactId>scireumOpen</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>net.sf.ehcache</groupId>
			<artifactId>ehcache</artifactId>
			<version>2.7.4</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>commons-lang</groupId>
			<artifactId>commons-lang</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>org.eclipse.osgi</groupId>
			<artifactId>org.eclipse.osgi</artifactId>
			<version>3.7.1</version>
		</dependency>		
	</dependencies>
	<profiles>
		<profile>
			<id>win_x86</id>
			<build>
				<directory>${project.basedir}/target/win_x86</directory>
			</build>
			<properties>
				<swt.version>org.eclipse.swt.win32.win32.x86</swt.version>
			</properties>
		</profile>
		<profile>
			<id>win_x86_64</id>
			<build>
				<directory>${project.basedir}/target/win_x86_64</directory>
			</build>
			<properties>
				<swt.version>org.eclipse.swt.win32.win32.x86_64</swt.version>
			</properties>
		</profile>
		<profile>
			<id>macos_x86</id>
			<build>
				<directory>${project.basedir}/target/macos_x86</directory>
			</build>
			<properties>
				<swt.version>org.eclipse.swt.cocoa.macosx</swt.version>
			</properties>
		</profile>
		<profile>
