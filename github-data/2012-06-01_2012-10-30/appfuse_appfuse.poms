    <build>
        <plugins>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.7</version>
                <executions>
                    <execution>
                        <id>build-archetype</id>
                        <phase>package</phase>
                        <configuration>
                            <tasks>
                                <delete dir="target/${module}-archetype" />
                                <ant antfile="../build.xml" dir="." target="build-archetype">
                                    <property name="module" value="${module}" />
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test-archetype</id>
                        <phase>integration-test</phase>
                        <configuration>
                            <tasks>
                                <ant antfile="../build.xml" dir="." target="test">
                                    <property name="archetype" value="${project.artifactId}-archetype" />
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>deploy-archetype</id>
                        <phase>deploy</phase>
                        <configuration>
                            <tasks>
                                <ant antfile="../build.xml" dir="." target="deploy">
                                    <property name="module" value="${module}" />
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

