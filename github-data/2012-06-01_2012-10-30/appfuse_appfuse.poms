    <artifactId>appfuse-modular-wicket</artifactId>
    <name>AppFuse Modular Archetype - Wicket</name>
    <packaging>pom</packaging>
    <version>3.0.1-SNAPSHOT</version>

    <description>
        Maven 2 archetype that creates a modular web application with AppFuse. This archetype creates two modules:
        "core" and "web". The core module depends on appfuse-service, while the web module depends on core as well
        as AppFuse's Wicket implementation.
    </description>

    <scm>
        <connection>scm:git:git@github.com:appfuse/appfuse.git</connection>
        <developerConnection>scm:git:git@github.com:appfuse/appfuse.git</developerConnection>
        <url>http://source.appfuse.org/browse/appfuse/archetypes/appfuse-modular-wicket</url>
      <tag>HEAD</tag>
  </scm>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.7</version>
                <executions>
                    <execution>
                        <id>build-archetype</id>
                        <phase>package</phase>
                        <configuration>
                            <tasks> <!-- target? -->
                                <delete dir="target/${module}-archetype" />
                                <mkdir dir="target/${module}-archetype/core" />
                                <copy todir="target/${module}-archetype/core">
                                    <fileset dir="../appfuse-core/src">
                                        <exclude name="pom.xml" />
                                        <exclude name="README.txt" />
                                    </fileset>
                                </copy>
                                <!-- Fix bug that happens in Bamboo -->
                                <delete dir="target/${module}-archetype/core" includes="main/**" />

                                <ant antfile="../build.xml" dir="." target="build-archetype">
                                    <property name="module" value="${module}" />
                                    <property name="dest" value="web" />
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test-archetype</id>
                        <phase>integration-test</phase>
                        <configuration>
                            <tasks>
                                <ant antfile="../build.xml" dir="." target="test">
                                    <property name="archetype" value="${project.artifactId}-archetype" />
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>deploy-archetype</id>
                        <phase>deploy</phase>
                        <configuration>
                            <tasks>
                                <ant antfile="../build.xml" dir="." target="deploy">
                                    <property name="module" value="${module}" />
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

    <properties>
        <module>wicket</module>
    </properties>
