    <packaging>war</packaging>
    <version>1.0-SNAPSHOT</version>
    <name>AppFuse GWT Application</name>
    <url>http://www.mycompany.com</url>

    <prerequisites>
        <maven>3.1.0</maven>
    </prerequisites>

