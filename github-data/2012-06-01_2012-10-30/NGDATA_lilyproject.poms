      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-checkstyle-plugin</artifactId>
        <version>2.9.1</version>
        <configuration>
          <configLocation>org/lilyproject/checkstyle.xml</configLocation>
          <headerLocation>org/lilyproject/header.txt</headerLocation>
          <suppressionsLocation>org/lilyproject/checkstyle-suppressions.xml</suppressionsLocation>
          <aggregate>true</aggregate>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>findbugs-maven-plugin</artifactId>
        <version>2.5.2</version>
        <configuration>
          <!--
            <aggregate>true</aggregate> does nothing; Findbugs doesn't support aggregate reports, see http://mojo.codehaus.org/findbugs-maven-plugin/faq.html; just keeping it here in case it ever makes a difference
          -->
          <aggregate>true</aggregate>
        </configuration>
      </plugin>
      <!-- disabled for now because the output repeats each warning many times, resulting
           in annoyingly large files
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-pmd-plugin</artifactId>
       <version>3.0</version>
       <configuration>
         <aggregate>true</aggregate>
         <targetJdk>1.6</targetJdk>
