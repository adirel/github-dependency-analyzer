                  <condition property="key" value="${project.version}-${build.time}" else="${project.version}">
                    <contains string="${project.version}" substring="-SNAPSHOT" />
                  </condition>
                  <echo file="${basedir}/target/classes/LILY-INF/conf/build.xml" encoding="utf8"><![CDATA[<?xml version="1.0"?>
<!-- generated by the module build process -->
<conf>
  <build>
    <id>${project.id}</id>
    <by>${user.name}</by>
    <time>${build.time}</time>
    <version>${project.version}</version>
    <key>${key}</key>
    <java>
      <vendor>${java.vendor}</vendor>
      <version>${java.version}</version>
    </java>    
  </build>
</conf>]]></echo>
                </tasks>
              </configuration>
            </execution>
          </executions>
        </plugin>
      </plugins>      
    </pluginManagement>

  </build>

</project>
