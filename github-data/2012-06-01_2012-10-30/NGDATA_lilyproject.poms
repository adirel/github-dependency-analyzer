    <pluginManagement>
      <plugins>
        <!--  lily-runtime module building :: generating classloader.xml -->
        <plugin>
          <groupId>org.lilyproject</groupId>
          <artifactId>lily-runtime-genclassloader-plugin</artifactId>
          <version>2.8-SNAPSHOT</version>
          <executions>
            <execution>
              <phase>compile</phase>
              <goals>
                <goal>generate</goal>
              </goals>
            </execution>
          </executions>
        </plugin>

        <!--  lily-runtime module building :: producing a conf/build.xml -->
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-antrun-plugin</artifactId>
          <executions>
            <execution>
              <phase>generate-resources</phase>
              <goals>
                <goal>run</goal>
              </goals>
              <configuration>
                <tasks>
