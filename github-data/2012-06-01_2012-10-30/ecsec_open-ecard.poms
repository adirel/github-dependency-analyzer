		    </zipalign>
		    <sdk>
			<platform>12</platform>
		    </sdk>
		    <test>
			<createReport>true</createReport>
		    </test>
		    <proguard>
			<skip>${proguard.disabled}</skip>
		    </proguard>
		</configuration>
