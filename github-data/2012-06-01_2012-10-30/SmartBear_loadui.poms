                                        <pathelement location="${env.JAVA_HOME}/lib/ant-javafx.jar"/>
                                        <pathelement location="${env.JAVA_HOME}/jre/lib/ext/jfxrt.jar"/>
                                    </classpath>
                                </taskdef>

                                <fx:csstobin outdir="${basedir}/target/classes">
                                    <fileset dir="src/main/java" includes="**/*.css"/>
                                </fx:csstobin>

                                <fx:csstobin outdir="${basedir}/target/classes">
                                    <fileset dir="src/main/resources" includes="**/*.css"/>
                                </fx:csstobin>
                            </target>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <excludes>
                    <exclude>**/*.css</exclude>
                </excludes>
            </resource>
            <resource>
                <directory>src/main/java</directory>
                <includes>
