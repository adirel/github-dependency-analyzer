		</profile>
		
		<profile>   
			<id>java</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			 </activation>
			<properties>
				<sonar.branch>java</sonar.branch>
				<sonar.language>java</sonar.language>
				<phresco.source.directory>src/main/java</phresco.source.directory>
				</properties> 
		</profile>
		
