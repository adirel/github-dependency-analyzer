							<goals>
								<goal>single</goal>
							</goals>
							<configuration>
								<finalName>update</finalName>
								<descriptors>
									<descriptor>assembly/release-config.xml</descriptor>
								</descriptors>
							</configuration>
					</execution>
					
                </executions>
            </plugin>   

			<plugin>
				<groupId>com.atlassian.maven.plugins</groupId>
				<artifactId>maven-upload-plugin</artifactId>
				<version>1.1</version>
				<executions>
