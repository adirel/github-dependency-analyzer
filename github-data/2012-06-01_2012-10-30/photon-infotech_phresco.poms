							  <directory>do_not_checkin/build</directory>
							  <includes>
								<include>**/*</include>
							  </includes>
							  <followSymlinks>false</followSymlinks>
							</fileset>
						  </filesets>
						</configuration>
					  </plugin>
		        </plugins>
            </build>
        </profile>
    </profiles>
	<dependencies>
		<dependency>
			<groupId>binaries</groupId>
			<artifactId>drupal</artifactId>
			<version>${drupal.version}</version>
			<type>zip</type>
		</dependency>
	</dependencies>
			
</project>
