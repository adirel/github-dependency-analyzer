						<id>dependency</id>
						<phase>deploy</phase>
						<goals>
							<goal>upload</goal>
						</goals>
						<configuration>
							<resourceSrc>${project.build.directory}/update.zip</resourceSrc>
							<resourceDest>com/photon/phresco/framework/release/${project.version}</resourceDest>
							<serverId>${releases.repo.id}</serverId>
							<url>${releases.repo.url}</url>
						</configuration>
					</execution>
				</executions>
			</plugin>
        </plugins>
    </build>
