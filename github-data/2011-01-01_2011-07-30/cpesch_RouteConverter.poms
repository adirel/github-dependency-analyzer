                <groupId>com.vineetmanohar</groupId>
                <artifactId>maven-twitter-plugin</artifactId>
                <version>0.1</version>
                <executions>
                    <execution>
                        <configuration>
                            <twitterStatus>Version ${project.version} of RouteConverter released to http://www.routeconverter.com</twitterStatus>
                        </configuration>
                        <id>deploy</id>
                        <phase>deploy</phase>
                        <goals>
                            <goal>tweet</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

