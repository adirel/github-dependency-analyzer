                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <finalName>${distro.file}</finalName>
                            <descriptors>
                                <descriptor>${basedir}/assembly.xml</descriptor>
                            </descriptors>
                        </configuration>
                    </execution>
                    <execution>
                        <id>assembly-full-dist</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <finalName>${distro.file.final}</finalName>
                            <descriptors>
                                <descriptor>${basedir}/assembly-final.xml</descriptor>
                            </descriptors>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <!-- Configure the Surefire plugin for running the tests.  Be sure to make these same setting if running in your IDE... -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <argLine>-Xms512m -Xmx512m</argLine>
                    <environmentVariables>
                        <JBOSS_HOME>${jboss.home}</JBOSS_HOME>
                        <SWITCHYARD_VERSION>${project.version}</SWITCHYARD_VERSION>
                    </environmentVariables>
                    <systemProperties>
                        <property>
                            <name>jboss.home</name>
                            <value>${jboss.home}</value>
                        </property>
                        <property>
                            <name>module.path</name>
                            <value>${jboss.home}/modules</value>
                        </property>
                        <property>
                            <name>server-config</name>
                            <value>standalone-full.xml</value>
                        </property>
                    </systemProperties>
                    <trimStackTrace>false</trimStackTrace>
                    <printSummary>true</printSummary>
                    <forkMode>always</forkMode>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <profiles>
        <profile>
            <id>default</id>
            <activation>
                <property>
                    <name>!jboss.home</name>
                </property>
            </activation>
            <properties>
                <jboss.home>${switchyard.jboss.home}</jboss.home>
            </properties>
        </profile>
        <profile>
            <id>finalGa</id>
            <activation>
                <property>
                    <name>jboss.home</name>
                </property>
                <file>
                    <exists>${jboss.home}/modules/system/layers/base/org/jboss/ws/spi/main/jbossws-spi-2.1.2.Final-redhat-1.jar</exists>
                </file>
            </activation>
            <properties>
                <zip.file.name>${distro.file.final}.zip</zip.file.name>
            </properties>
        </profile>
        <profile>
            <id>manual</id>
            <activation>
                <property>
                    <name>jboss.home</name>
                </property>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>prepare</id>
                                <phase>process-test-classes</phase>
                                <configuration>
                                    <tasks>
                                        <unzip dest="${jboss.home}" src="${project.build.directory}/${zip.file.name}"/>
                                        <copy todir="${jboss.home}/standalone/configuration" file="${staging.jboss.dir}/standalone/configuration/application-users.properties"/>
                                        <copy todir="${jboss.home}/standalone/configuration" file="${staging.jboss.dir}/standalone/configuration/application-roles.properties"/>
                                    </tasks>
                                </configuration>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
</project>
