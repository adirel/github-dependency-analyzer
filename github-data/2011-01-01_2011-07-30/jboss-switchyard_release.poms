                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.switchyard</groupId>
                                    <artifactId>switchyard-wildfly-bundle</artifactId>
                                    <type>zip</type>
                                    <version>${project.version}</version>
