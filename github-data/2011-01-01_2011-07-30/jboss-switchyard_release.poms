    <build>
        <testResources>
            <testResource>
                <directory>src/test/resources</directory>
                <filtering>true</filtering>
            </testResource>
        </testResources>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>stage-jboss</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.jboss.as</groupId>
                                    <artifactId>jboss-as-dist</artifactId>
                                    <type>zip</type>
                                </artifactItem>
                            </artifactItems>
                            <outputDirectory>${staging.dir}</outputDirectory>
                        </configuration>
                    </execution>
                    <execution>
                        <id>stage-jboss-configs</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.jboss.as</groupId>
                                    <artifactId>jboss-as-dist</artifactId>
                                    <type>zip</type>
                                    <overWrite>true</overWrite>
                                </artifactItem>
                            </artifactItems>
                            <includes>${redhat.eap6.dir}/standalone/configuration/standalone.xml, ${redhat.eap6.dir}/standalone/configuration/standalone-full.xml, ${redhat.eap6.dir}/standalone/configuration/standalone.xml, ${redhat.eap6.dir}/standalone/configuration/standalone-ha.xml, ${redhat.eap6.dir}/standalone/configuration/standalone.xml, ${redhat.eap6.dir}/standalone/configuration/standalone-full-ha.xml,${redhat.eap6.dir}/domain/configuration/domain.xml</includes>
                            <outputDirectory>${staging.configs.dir}</outputDirectory>
                        </configuration>
                    </execution>
                    <execution>
                        <id>unpack-bundle</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.switchyard</groupId>
                                    <artifactId>switchyard-as7-standalone-bundle</artifactId>
                                    <type>zip</type>
                                    <version>${project.version}</version>
                                </artifactItem>
                            </artifactItems>
                            <outputDirectory>${staging.jboss.dir}</outputDirectory>
                        </configuration>
                    </execution>
                    <execution>
                        <id>unpack-tests</id>
                        <phase>process-test-classes</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.switchyard</groupId>
                                    <artifactId>switchyard-as7-tests</artifactId>
                                    <type>test-jar</type>
                                    <version>${project.version}</version>
                                </artifactItem>
                            </artifactItems>
                            <excludes>application-roles.properties, application-users.properties</excludes>
                            <outputDirectory>${project.build.testOutputDirectory}</outputDirectory>
                        </configuration>
                    </execution>
                    <execution>
                        <id>unpack-users</id>
                        <phase>process-test-classes</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.switchyard</groupId>
                                    <artifactId>switchyard-as7-tests</artifactId>
                                    <type>test-jar</type>
                                    <version>${project.version}</version>
                                </artifactItem>
                            </artifactItems>
                            <includes>application-roles.properties, application-users.properties</includes>
                            <outputDirectory>${staging.jboss.dir}/standalone/configuration</outputDirectory>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>xml-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>transform</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <forceCreation>true</forceCreation>
                    <transformationSets>
                        <transformationSet>
                            <dir>${staging.configs.dir}/${redhat.eap6.dir}/standalone/configuration</dir>
                            <includes>
                                <include>standalone.xml</include>
                                <include>standalone-ha.xml</include>
                                <include>standalone-full-ha.xml</include>
                                <include>standalone-full.xml</include>
                            </includes>
                            <outputDir>${staging.jboss.dir}/standalone/configuration</outputDir>
                            <stylesheet>${staging.jboss.dir}/xsl/standalone.xsl</stylesheet>
                        </transformationSet>
                        <transformationSet>
                            <dir>${staging.jboss.dir}/standalone/configuration</dir>
                            <includes>
                                <include>standalone-ha.xml</include>
                                <include>standalone-full-ha.xml</include>
                            </includes>
                            <outputDir>${staging.jboss.dir}/standalone/configuration</outputDir>
                            <stylesheet>${staging.jboss.dir}/xsl/standalone-ha.xsl</stylesheet>
                        </transformationSet>
                        <transformationSet>
                            <dir>${staging.configs.dir}/${redhat.eap6.dir}/domain/configuration</dir>
                            <includes>
                                <include>domain.xml</include>
                            </includes>
                            <outputDir>${staging.jboss.dir}/domain/configuration</outputDir>
                            <stylesheet>${staging.jboss.dir}/xsl/domain.xsl</stylesheet>
                        </transformationSet>
                        <transformationSet>
                            <dir>${staging.jboss.dir}/modules/system/layers/base/org/jboss/ws/spi/main</dir>
                            <includes>
                                <include>module.xml</include>
                            </includes>
                            <outputDir>${staging.jboss.dir}/modules/system/layers/base/org/jboss/ws/spi/main</outputDir>
                            <stylesheet>${staging.jboss.dir}/xsl/jbossws-spi_module.xsl</stylesheet>
                        </transformationSet>
                        <transformationSet>
                            <dir>${staging.jboss.dir}/modules/system/layers/base/org/jboss/as/webservices/main</dir>
                            <includes>
                                <include>module.xml</include>
                            </includes>
                            <outputDir>${staging.jboss.dir}/modules/system/layers/base/org/jboss/as/webservices/main</outputDir>
                            <stylesheet>${staging.jboss.dir}/xsl/jbossws-jboss720-server-integration_module.xsl</stylesheet>
                        </transformationSet>
                    </transformationSets>
                </configuration>
