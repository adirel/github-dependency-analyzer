						<artifactId>cobertura-maven-plugin</artifactId>
					</plugin>
				</plugins>
			</reporting>
		</profile>
		<profile>
			<id>examples</id>
			<modules>
				<module>examples</module>
			</modules>
		</profile>
	</profiles>
</project>
