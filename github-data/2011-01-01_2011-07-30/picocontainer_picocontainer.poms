                    <outputDirectory>target/test-classes</outputDirectory>
                </configuration>
                <executions>
                    <execution>
                        <phase>test</phase>
                        <id>generate</id>
                        <goals>
                            <goal>generate</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
			<plugin>
