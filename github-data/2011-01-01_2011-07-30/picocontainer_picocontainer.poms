            <groupId>ch.qos.logback</groupId><artifactId>logback-core</artifactId><version>1.0.9</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>ch.qos.logback</groupId><artifactId>logback-classic</artifactId><version>1.0.9</version>
            <scope>provided</scope>
        </dependency>
