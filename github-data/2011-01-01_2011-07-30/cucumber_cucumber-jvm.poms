def template = """\
module Cucumber
  module Runtime
    module JRuby
      module Dsl
<% i18n.codeKeywords.each { kw -> %>
        def \${java.text.Normalizer.normalize(kw, java.text.Normalizer.Form.NFC)}(regexp, &proc)
          register_stepdef(regexp, proc)
        end
<% } %>
      end
    end
  end
end
"""

gherkin.I18n.all.each { i18n ->
