  <artifactId>target-platforms</artifactId>
  <packaging>pom</packaging>
  <parent>
  	<groupId>org.eclipse.swtbot</groupId>
  	<artifactId>parent</artifactId>
  	<version>2.2.2-SNAPSHOT</version>
  	<relativePath>../../pom.xml</relativePath>
