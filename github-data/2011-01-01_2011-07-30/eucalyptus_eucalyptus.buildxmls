	  <!-- copy udev config over -->
	  <mkdir dir="${DESTDIR}${euca.udev.dir}" />
	  <copy todir="${DESTDIR}${euca.udev.dir}" overwrite="true">
	    <fileset dir="${basedir}/udev">
	      <include name="**/*" />
	    </fileset>
	  </copy>
	</target>
</project>
