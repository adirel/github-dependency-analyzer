            <goals>
              <goal>enhance</goal>
            </goals>
            <configuration>
              <metadataIncludes>com/force/sdk/jpa/model/**/*.class</metadataIncludes>
              <mappingIncludes>com/force/sdk/jpa/model/**/*.class</mappingIncludes>
