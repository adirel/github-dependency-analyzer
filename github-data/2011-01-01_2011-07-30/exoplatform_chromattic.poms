  <description>Chromattic Framework Reference Guide</description>

  <properties>
    <wikbook.version>0.9.36</wikbook.version>
    <wikbook.source>${pom.basedir}/src/main/wikbook</wikbook.source>
    <wikbook.target>${project.build.directory}/wikbook</wikbook.target>
  </properties>

  <dependencies>
    <dependency>
      <groupId>org.wikbook</groupId>
      <artifactId>wikbook.docbkxstyle</artifactId>
      <version>${wikbook.version}</version>
    </dependency>

        <dependency>
          <groupId>org.chromattic</groupId>
          <artifactId>chromattic.api</artifactId>
          <version>1.3.1-beta1-SNAPSHOT</version>
        </dependency>
        <dependency>
          <groupId>org.chromattic</groupId>
          <artifactId>chromattic.api</artifactId>
          <version>1.3.1-beta1-SNAPSHOT</version>
