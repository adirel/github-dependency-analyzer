  <profiles>

    <profile>
      <id>repo</id>
      <activation>
        <property>
          <name>!skip-repo</name>
        </property>
      </activation>
      <distributionManagement>
        <repository>
          <id>repository.exoplatform.org</id>
          <url>${chromattic.releases.repo.url}</url>
        </repository>
        <snapshotRepository>
          <id>repository.exoplatform.org</id>
          <url>${chromattic.snapshots.repo.url}</url>
        </snapshotRepository>
      </distributionManagement>
    </profile>

     <profile>
        <id>Java5</id>
        <activation>
           <jdk>1.5</jdk>
        </activation>
        <modules>
           <module>test</module>
           <module>api</module>
           <module>spi</module>
           <module>common</module>
           <module>metamodel</module>
