                <phase>site</phase>
                <goals>
                    <goal>instrument</goal>
                    <goal>cobertura</goal>
                </goals>
            </execution>
        </executions>
        <configuration>
          <aggregate>true</aggregate>
          <instrumentation>
            <excludes>
