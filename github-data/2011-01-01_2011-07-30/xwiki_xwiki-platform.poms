     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-remote-resources-plugin</artifactId>
       <executions>
         <execution>
           <goals>
             <goal>bundle</goal>
           </goals>
         </execution>
       </executions>
     </plugin>
      <!-- Add specific excludes for this module for license checks -->
      <plugin>
        <groupId>com.mycila</groupId>
        <artifactId>license-maven-plugin</artifactId>
        <configuration>
          <excludes>
            <!-- Exclude Velocity generated files ending with "vm" since they end with vm but their contents are not
                 Velocity templates and the license plugin would validate their licenses against the vm style.
                 TODO: use native extensions instead of "*.vm" -->
            <exclude>**/*.cfg.xml.vm</exclude>
            <exclude>**/*.properties.vm</exclude>
            <exclude>**/*.cfg.vm</exclude>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
