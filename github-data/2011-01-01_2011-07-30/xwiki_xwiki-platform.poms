    <dependency>
      <groupId>org.xwiki.rendering</groupId>
      <artifactId>xwiki-rendering-transformation-linkchecker</artifactId>
      <version>${rendering.version}</version>
    </dependency>

    <!-- Test Dependencies -->
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>servlet-api</artifactId>
      <scope>test</scope>
    </dependency>
    <!-- Since we're mocking a class (PircBotX, see http://code.google.com/p/pircbotx/issues/detail?id=62) we need
         those -->
    <dependency>
      <groupId>org.jmock</groupId>
      <artifactId>jmock-legacy</artifactId>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <!-- TODO: Remove this once this bug is fixed: http://code.google.com/p/pircbotx/issues/detail?id=114 -->
      <plugin>
        <groupId>org.jacoco</groupId>
        <artifactId>jacoco-maven-plugin</artifactId>
        <configuration>
          <excludes>
            <exclude>org.pircbotx.hooks.ListenerAdapter</exclude>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
