                        <type>war</type>
                      </dependency>
                      <contextPath>/</contextPath>
                    </warRunDependency>
                  </warRunDependencies>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
  </profiles>

