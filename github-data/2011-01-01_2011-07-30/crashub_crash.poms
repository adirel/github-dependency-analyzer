              <goals>
                <goal>single</goal>
              </goals>
              <configuration>
                <classifier>standalone</classifier>
                <ignoreMissingDescriptor>true</ignoreMissingDescriptor>
                <descriptors>
                  <descriptor>src/main/assembly/standalone.xml</descriptor>
                </descriptors>
              </configuration>
            </execution>
            <execution>
              <id>make-packaging</id>
              <phase>package</phase>
              <goals>
                <goal>single</goal>
              </goals>
              <configuration>
                <appendAssemblyId>false</appendAssemblyId>
                <ignoreMissingDescriptor>true</ignoreMissingDescriptor>
