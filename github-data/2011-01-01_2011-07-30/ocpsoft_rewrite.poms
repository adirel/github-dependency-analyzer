                        <configuration>
                           <target>
                              <mkdir dir="${container.download.dir}" />
                              <get dest="${container.download.dir}" verbose="true" skipexisting="true" src="http://download.java.net/glassfish/${version.glassfish3}/release/glassfish-${version.glassfish3}-web.zip" />
