			</dependency>

			<dependency>
				<groupId>org.solovyev</groupId>
				<artifactId>jscl</artifactId>
				<version>${jscl.version}</version>
				<exclusions>
					<exclusion>
						<artifactId>xercesImpl</artifactId>
