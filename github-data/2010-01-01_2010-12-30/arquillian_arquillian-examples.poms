   <modelVersion>4.0.0</modelVersion>

   <groupId>org.jboss.arquillian.examples</groupId>
   <artifactId>jsfunit-servlet</artifactId>
   <version>1.0.0-SNAPSHOT</version>
   <packaging>jar</packaging>
   <name>Arquillian Examples: JSFUnit on a Servlet</name>
   <description>Examples that demonstrate how to test JSF pages using Arquillian and JSFUnit in a Servlet environment</description>

   <properties>
      <version.arquillian>1.0.0.Alpha5</version.arquillian>
      <version.jetty>8.0.0.M1</version.jetty>
