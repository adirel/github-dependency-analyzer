      </dependency>
      <dependency>
         <groupId>org.jboss.arquillian.framework</groupId>
         <artifactId>arquillian-framework-jsfunit</artifactId>
         <version>${version.arquillian}</version>
      </dependency>
      <dependency>
         <groupId>junit</groupId>
         <artifactId>junit</artifactId>
