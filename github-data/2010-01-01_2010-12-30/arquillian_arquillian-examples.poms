            <dependency>
               <groupId>org.jboss.arquillian.container</groupId>
               <artifactId>arquillian-tomcat-embedded-6</artifactId>
               <version>${version.arquillian}</version>
               <scope>test</scope>
            </dependency>
            <dependency>
               <groupId>org.apache.tomcat</groupId>
               <artifactId>catalina</artifactId>
               <version>${version.tomcat}</version>
               <scope>test</scope>
            </dependency>
            <dependency>
               <groupId>org.apache.tomcat</groupId>
               <artifactId>coyote</artifactId>
               <version>${version.tomcat}</version>
               <scope>test</scope>
            </dependency>
            <dependency>
               <groupId>org.apache.tomcat</groupId>
               <artifactId>jasper</artifactId>
               <version>${version.tomcat}</version>
               <scope>test</scope>
            </dependency>
            <dependency>
               <groupId>com.sun.faces</groupId>
               <artifactId>jsf-impl</artifactId>
               <version>2.0.3</version>
               <scope>test</scope>
            </dependency>
            <dependency>
               <groupId>org.glassfish.web</groupId>
               <artifactId>el-impl</artifactId>
               <version>2.2</version>
               <scope>test</scope>
            </dependency>
         </dependencies>
         <build>
            <testResources>
               <testResource>
                  <directory>src/test/resources</directory>
               </testResource>
               <testResource>
                  <directory>src/test/resources-tomcat</directory>
               </testResource>
            </testResources>
         </build>
      </profile>
      <profile>
         <!--
         This profile will copy all the test dependencies to target/test-libs to help
         those not using Maven understand what dependencies they need on the classpath
         -->
         <id>copy-test-libs</id>
         <build>
            <plugins> 
               <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-dependency-plugin</artifactId>
                  <version>2.1</version>
                  <executions>
                     <execution>
                        <id>copy-test-libs</id>
                        <phase>process-test-resources</phase>
                        <configuration>
                           <includeScope>test</includeScope>
                           <outputDirectory>${project.build.directory}/test-libs</outputDirectory>
                           <stripVersion>true</stripVersion>
                        </configuration>
                        <goals>
                           <goal>copy-dependencies</goal>
                        </goals>
                     </execution>
                  </executions>
               </plugin>
            </plugins> 
         </build>
      </profile>
   </profiles>

