                                <configuration>
                                    <artifactItems>
                                        <artifactItem><groupId>org.jacoco</groupId><artifactId>org.jacoco.ant</artifactId><version>${version.jacoco.plugin}</version></artifactItem>
                                    </artifactItems>
                                    <stripVersion>true</stripVersion>
