         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.wildfly</groupId>
        <artifactId>wildfly-parent</artifactId>
        <version>8.1.0.CR1</version>
    </parent>

    <artifactId>wildfly-build-modular</artifactId>

    <name>WildFly: Build Modular</name>
    <packaging>jar</packaging>

    <properties>
        <!-- generated configs root dirs -->
        <generated.configs>${basedir}/target/generated-configs</generated.configs>
        <generated.configs.standalone>${generated.configs}/standalone/configuration</generated.configs.standalone>
        <generated.configs.src.dir>${basedir}/../build/src/main/resources</generated.configs.src.dir>
    </properties>
    
    <dependencies>
    	<dependency>
    		<groupId>org.wildfly</groupId>
    		<artifactId>wildfly-config-assembly</artifactId>
    	</dependency>
    </dependencies>
        
    <profiles>
        <profile>
            <id>modular-minimal</id>
            <activation>
                <property>
                	<name>modular</name>
                	<value>minimal</value>
                </property>
            </activation>
            <properties>
            	<modular.config.name>minimal</modular.config.name>
            	<standalone.subsystems>logging:minimal,deployment-scanner</standalone.subsystems>
            	<standalone.template>configuration/standalone/template.xml</standalone.template>
            	<console.enabled>true</console.enabled>
            	<domain.enabled>false</domain.enabled>
            </properties>
            <build>
		        <plugins>
		            <plugin>
		                <groupId>org.apache.maven.plugins</groupId>
		                <artifactId>maven-antrun-plugin</artifactId>
		                <inherited>false</inherited>
		                <executions>
		                    <execution>
		                        <id>modular-config</id>
		                        <goals>
		                            <goal>run</goal>
		                        </goals>
		                        <phase>generate-resources</phase>
		                        <configuration>
		                            <target>
		                                <ant antfile="src/main/resources/build-modular-config.xml" target="all" inheritRefs="true"/>
		                            </target>
		                        </configuration>
		                    </execution>
		                </executions>
		            </plugin>
		        </plugins>
            </build>
        </profile>
    </profiles>

</project>
