        <profile>
            <id>test-reporting</id>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions combine.self="override">

                            <!-- Run the reporting tool. -->
                            <execution>
                                <id>reporting</id>
