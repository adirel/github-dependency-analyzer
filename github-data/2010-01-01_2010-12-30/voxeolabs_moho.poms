        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.6.1</version>
        <executions>
          <execution>
            <id>attach-javadocs</id>
            <configuration>
              <javadocDirectory>
                ${basedir}/src/main/javadoc/
