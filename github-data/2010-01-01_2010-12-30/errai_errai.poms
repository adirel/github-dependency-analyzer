          <soyc>false</soyc>
          <hostedWebapp>src/main/webapp/</hostedWebapp>                
        </configuration>
        <executions>
          <execution>
            <id>gwt-clean</id>
            <phase>clean</phase>
            <goals>
              <goal>clean</goal>
            </goals>
          </execution>
