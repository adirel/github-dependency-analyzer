                                <dependency-source>javax.enterprise:*</dependency-source>
                                <dependency-source>javax.inject:*</dependency-source>
                            </dependencySourceIncludes>
                        </configuration>
                        <executions>
                            <execution>
                                <id>aggregate-jar</id>
                                <goals>
                                    <goal>aggregate-jar</goal>
                                </goals>
                                <phase>package</phase>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

