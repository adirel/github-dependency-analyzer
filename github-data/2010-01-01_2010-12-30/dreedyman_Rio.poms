                        <exclude>**/*IT*.java</exclude>
                    </excludes>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <profiles>
        <profile>
            <id>run-integration-test</id>
            <activation>
                <file>
                    <exists>${env.RIO_HOME}</exists>
                </file>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-failsafe-plugin</artifactId>
                        <configuration>
                            <forkMode>always</forkMode>
                            <systemProperties>
                                <property>
                                    <name>java.security.policy</name>
                                    <value>${env.RIO_HOME}/policy/policy.all</value>
                                </property>
                            </systemProperties>
                            <excludes>
                                <exclude>**/*ITWorkflowTest.java</exclude>
                            </excludes>
                        </configuration>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>integration-test</goal>
                                    <goal>verify</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>

                    <plugin>
                        <artifactId>maven-surefire-plugin</artifactId>
                        <configuration>
                            <systemProperties>
                                <property>
                                    <name>java.security.policy</name>
                                    <value>${env.RIO_HOME}/policy/policy.all</value>
                                </property>
                                <property>
                                    <name>opstring</name>
                                    <value>../src/main/opstring/workflow.groovy</value>
                                </property>
                            </systemProperties>
                            <excludes>
                                <exclude>**/*ITWorkflowDeployTest.java</exclude>
                            </excludes>
                        </configuration>
                    </plugin>
                </plugins>


            </build>
        </profile>
    </profiles>

</project>
