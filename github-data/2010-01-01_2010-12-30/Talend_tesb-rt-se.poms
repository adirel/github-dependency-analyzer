                                        <replacevalue></replacevalue>
                                    </replacefilter>
                                    <replacefilter>
                                        <replacetoken>&lt;bundle dependency='true'&gt;mvn:org.apache.servicemix.bundles/org.apache.servicemix.bundles.xerces/2.11.0_1&lt;/bundle&gt;</replacetoken> 
                                        <replacevalue></replacevalue>
                                    </replacefilter>
                                </replace>
