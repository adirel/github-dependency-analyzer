					<artifactId>qcadoo-plugins-plugin-management</artifactId>
					<version>${qcadoo.version}</version>
				</dependency>
			</dependencies>
		</profile>
		<profile>
			<id>saas</id>
			<activation>
				<property>
					<name>spring.profiles.active</name>
					<value>saas</value>
				</property>
			</activation>
			<dependencies>
				<dependency>
					<groupId>com.qcadoo.saas</groupId>
					<artifactId>qcadoo-saas-plugins-plugin-management</artifactId>
					<version>${qcadoo.saas.version}</version>
				</dependency>
			</dependencies>
		</profile>
		<profile>
			<id>tomcat</id>
			<build>
				<plugins>
					<plugin>
						<groupId>com.qcadoo</groupId>
						<artifactId>qcadoo-maven-plugin</artifactId>
						<version>${qcadoo.maven.plugin.version}</version>
						<configuration>
							<jdbcDriver>${basedir}/driver/postgresql-9.2-1002.jdbc4.jar</jdbcDriver>
							<aspectJWeaver>${basedir}/driver/aspectjweaver-1.6.12.jar</aspectJWeaver>
							<configuration>${basedir}/conf/${profile.name}/</configuration>
							<rootFilesDirectory>${basedir}/root/</rootFilesDirectory>
						</configuration>
						<executions>
							<execution>
								<id>prepare-tomcat-package</id>
								<phase>package</phase>
								<goals>
									<goal>tomcat</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
