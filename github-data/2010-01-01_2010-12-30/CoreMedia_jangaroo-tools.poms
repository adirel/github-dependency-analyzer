<project xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
         xsi:schemaLocation='http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd'
         xmlns='http://maven.apache.org/POM/4.0.0'>
  <modelVersion>4.0.0</modelVersion>
  <groupId>testextxml</groupId>
  <artifactId>exml-only</artifactId>
  <packaging>pom</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>exml-only</name>
  <url>http://www.jangaroo.net</url>
  <build>
    <sourceDirectory>src/main/joo</sourceDirectory>
    <plugins>
      <plugin>
        <groupId>net.jangaroo</groupId>
        <artifactId>jangaroo-maven-plugin</artifactId>
        <version>@project.version@</version>
        <executions>
          <execution>
            <goals>
              <goal>compile</goal>
              <goal>package</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>net.jangaroo</groupId>
        <artifactId>exml-maven-plugin</artifactId>
        <extensions>true</extensions>
        <version>@project.version@</version>
        <executions>
          <execution>
            <goals>
              <goal>exml</goal>
              <goal>exml-xsd</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <configClassPackage>testPackage.config</configClassPackage>
          <validationMode>error</validationMode>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
