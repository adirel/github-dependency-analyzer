    <artifactId>deepamehta-provision</artifactId>
    <version>4.3-SNAPSHOT</version>
    <packaging>pom</packaging>

    <parent>
        <groupId>de.deepamehta</groupId>
        <artifactId>deepamehta-global</artifactId>
        <version>4.3-SNAPSHOT</version>
        <relativePath>../../pom.xml</relativePath>
    </parent>
    <!--
        POM projects with main dependency sets of the DeepaMehta 4 Standard Distribution.
        More precisely:
        1) The dm4-distribution POM depends on these POMs when assembling the Distribution.
           The assembly descriptor uses the project POM dependencies to transitive include artifacts.
        2) 3rd-party plugin development scenario:
           The dm4-plugin-parent Pax Runner does deploy the POM dependencies statically (not hot-deployed).
        3) Core development scenario:
           The global Pax Runner does NOT deploy DeepaMehta bundles as they are expected to be
           in the global POM's hot-deployment list (Felix File Install).
    -->
    <modules>
        <module>3rd-party-bundles</module>
        <module>felix-bundles</module>
        <module>deepamehta-bundles</module>
    </modules>
</project>
<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <name>DeepaMehta 4 Storage - Neo4j</name>
    <groupId>de.deepamehta</groupId>
    <artifactId>deepamehta-storage-neo4j</artifactId>
    <version>4.3-SNAPSHOT</version>
    <packaging>bundle</packaging>

    <parent>
        <groupId>de.deepamehta</groupId>
        <artifactId>deepamehta-parent</artifactId>
        <version>5</version>
        <relativePath>../dm4-parent/pom.xml</relativePath>
    </parent>

    <dependencies>
        <!-- DeepaMehta Core -->
        <dependency>
            <groupId>de.deepamehta</groupId>
            <artifactId>deepamehta-core</artifactId>
            <version>4.3-SNAPSHOT</version>
        </dependency>
        <!-- Neo4j -->
        <dependency>
            <groupId>org.neo4j</groupId>
            <artifactId>neo4j-kernel</artifactId>
            <version>1.8.1</version>
        </dependency>
        <dependency>
            <groupId>org.neo4j</groupId>
            <artifactId>neo4j-lucene-index</artifactId>
            <version>1.8.1</version>
        </dependency>
        <!-- JTA Specification (required by neo4j-kernel) -->
        <dependency>
            <groupId>org.apache.geronimo.specs</groupId>
            <artifactId>geronimo-jta_1.1_spec</artifactId>
            <version>1.1.1</version>
            <scope>runtime</scope>
        </dependency>
        <!-- Lucene Core (required by neo4j-lucene-index) -->
        <dependency>
            <groupId>org.apache.lucene</groupId>
            <artifactId>lucene-core</artifactId>
            <version>3.5.0</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <configuration>
                    <instructions>
                        <Bundle-Activator>
                            de.deepamehta.storage.neo4j.Neo4jStorageActivator
                        </Bundle-Activator>
                        <Import-Package>
                            !ch.qos.logback.*, !org.slf4j.*, !com.sun.*, !sun.misc.*, *
                        </Import-Package>
                        <_exportcontents><!-- export package without inlining the class files (we embed the jars) -->
                            org.neo4j.graphdb
                        </_exportcontents>
                        <Embed-Dependency>
                            neo4j-kernel, neo4j-lucene-index, geronimo-jta_1.1_spec, lucene-core
                        </Embed-Dependency>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
