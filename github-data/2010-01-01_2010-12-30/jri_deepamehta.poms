    <name>DeepaMehta 4 Core</name>
    <groupId>de.deepamehta</groupId>
    <artifactId>deepamehta-core</artifactId>
    <version>4.3-SNAPSHOT</version>
    <packaging>bundle</packaging>

    <parent>
        <groupId>de.deepamehta</groupId>
        <artifactId>deepamehta-parent</artifactId>
        <version>5</version>
        <relativePath>../dm4-parent/pom.xml</relativePath>
    </parent>

    <dependencies>
        <!-- OSGi -->
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>osgi_R4_core</artifactId>
        </dependency>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>osgi_R4_compendium</artifactId>
        </dependency>
        <!-- Jersey -->
        <dependency>
            <groupId>com.sun.jersey</groupId>
            <artifactId>jersey-servlet</artifactId>
        </dependency>
        <!-- Jettison -->
        <dependency>
            <groupId>org.codehaus.jettison</groupId>
            <artifactId>jettison</artifactId>
        </dependency>
        <!-- Servlet API -->
        <dependency>
            <groupId>javax.servlet</groupId>
            <artifactId>servlet-api</artifactId>
        </dependency>
        <!-- Felix HTTP API -->
        <dependency>
            <groupId>org.apache.felix</groupId>
            <artifactId>org.apache.felix.http.api</artifactId>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <configuration>
                    <instructions>
                        <Export-Package>
                            de.deepamehta.core,
                            de.deepamehta.core.model,
                            de.deepamehta.core.osgi,
                            de.deepamehta.core.service,
                            de.deepamehta.core.service.annotation,
                            de.deepamehta.core.service.event,
                            de.deepamehta.core.storage.spi,
                            de.deepamehta.core.util,
                            org.osgi.service.log        <!-- required when running without Gogo Shell -->
                        </Export-Package>
                        <Bundle-Activator>
                            de.deepamehta.core.osgi.CoreActivator
                        </Bundle-Activator>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
