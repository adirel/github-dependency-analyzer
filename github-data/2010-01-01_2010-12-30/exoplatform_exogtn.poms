                  </property>
                  <property>
                     <name>gatein.test.datasource.username</name>
                     <value>sa</value>
                  </property>
                  <property>
                     <name>gatein.test.datasource.password</name>
                     <value />
                  </property>
                </systemProperties>
             </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
    <!-- To activate mysql5 profile use: "-Pmysql5 -Dnohsqldb" -->
    <profile>
      <id>mysql5</id>
      <dependencies>
        <dependency>
          <groupId>mysql</groupId>
          <artifactId>mysql-connector-java</artifactId>
