        <site>
          <id>site-citrus</id>
          <url>scpexe://labs.consol.de/home/citrus/www/stage</url>
        </site>
      </distributionManagement>
      <properties>
        <site.url>http://www.citrusframework.org</site.url>
        <docbkx.img.path>images</docbkx.img.path>
      </properties>
    </profile>
    
    <profile>
      <id>release-local</id>
      <distributionManagement>
        <repository>
          <id>local-release</id>
          <url>file:///stage/repository</url>
        </repository>
        <snapshotRepository>
          <id>local-snapshots</id>
          <url>file:///stage/snapshots-repository</url>
        </snapshotRepository>
        <site>
          <id>site-citrus</id>
          <url>file:///stage/citrus-site</url>
        </site>
      </distributionManagement>
      <properties>
        <site.url>http://www.citrusframework.org</site.url>
        <docbkx.img.path>file:${basedir}/src/docbkx/images</docbkx.img.path>
      </properties>
    </profile>
    
    <profile>
      <id>site</id>
      <build>
        <plugins>
          <plugin>
            <artifactId>maven-site-plugin</artifactId>
            <dependencies>
              <dependency>
                <groupId>com.consol.citrus.mvn</groupId>
                <artifactId>citrus-default-skin</artifactId>
                <version>${project.version}</version>
              </dependency>
            </dependencies>
            <executions>
              <execution>
                <id>attach-descriptor</id>
                <goals>
                  <goal>attach-descriptor</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>com.agilejava.docbkx</groupId>
            <artifactId>docbkx-maven-plugin</artifactId>
            <executions>
              <execution>
                <id>html-reference-doc</id>
                <configuration>
                  <postProcess>
                    <copy todir="${project.basedir}/target/site/reference">
                      <fileset dir="${project.basedir}/target/docbkx">
                        <include name="**/*.html" />
                      </fileset>
                    </copy>
                    <copy todir="${project.basedir}/target/site/reference/html">
                      <fileset dir="${project.basedir}/src/docbkx">
                        <include name="**/*.css" />
                        <include name="**/*.png" />
                        <include name="**/*.gif" />
                        <include name="**/*.jpg" />
                      </fileset>
                    </copy>
                  </postProcess>
                </configuration>
                <goals>
                  <goal>generate-html</goal>
                </goals>
                <phase>pre-site</phase>
              </execution>
              <execution>
                <id>pdf-reference-doc</id>
                <configuration>
                  <postProcess>
                    <copy file="${project.basedir}/target/docbkx/pdf/index.pdf" 
                          tofile="${project.basedir}/target/site/reference/pdf/citrus-reference-${project.version}.pdf" failonerror="true" />
                  </postProcess>                    
                </configuration>
                <goals>
                  <goal>generate-pdf</goal>
                </goals>
                <phase>pre-site</phase>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <artifactId>maven-javadoc-plugin</artifactId>
            <version>2.7</version>
            <configuration>
              <aggregate>true</aggregate>
              <linksource>true</linksource>
              <links>
                <link>http://java.sun.com/j2se/1.5.0/docs/api/</link>
                <link>http://logging.apache.org/log4j/docs/api/</link>
                <link>http://static.springframework.org/spring/docs/2.5.x/api/</link>
                <link>http://testng.org/javadocs/</link>
              </links>
            </configuration>
          </plugin>
          <plugin>
            <artifactId>maven-resources-plugin</artifactId>
            <version>2.3</version>
            <inherited>false</inherited>
            <!-- TODO CD the artifact, rather than the path, should be used here -->
            <executions>
              <execution>
                <id>copy-citrus-core-schemas</id>
                <phase>post-site</phase>
                <configuration>
                  <outputDirectory>${project.build.directory}/site/schema/config</outputDirectory>
                  <resources>          
                    <resource>
                      <directory>modules/citrus-model/model-core/src/main/resources/com/consol/citrus/schema</directory>
                      <includes>
                        <include>citrus-config*.xsd</include>
                        <include>citrus-testcase*.xsd</include>
                      </includes>
                    </resource>
                  </resources>
                </configuration>
                <goals>
                  <goal>copy-resources</goal>
                </goals>
              </execution>
              <execution>
                <id>copy-citrus-http-schemas</id>
                <phase>post-site</phase>
                <configuration>
                  <outputDirectory>${project.build.directory}/site/schema/http/config</outputDirectory>
                  <resources>          
                    <resource>
                      <directory>modules/citrus-model/model-http/src/main/resources/com/consol/citrus/schema</directory>
                      <includes>
                        <include>citrus-http-config*.xsd</include>
                      </includes>
                    </resource>
                  </resources>
                </configuration>
                <goals>
                  <goal>copy-resources</goal>
                </goals>
              </execution>
              <execution>
                <id>copy-citrus-ws-schemas</id>
                <phase>post-site</phase>
                <configuration>
                  <outputDirectory>${project.build.directory}/site/schema/ws/config</outputDirectory>
                  <resources>          
                    <resource>
                      <directory>modules/citrus-model/model-ws/src/main/resources/com/consol/citrus/schema</directory>
                      <includes>
                        <include>citrus-ws-config*.xsd</include>
                        <include>citrus-ws-testcase*.xsd</include>
                      </includes>
                    </resource>
                  </resources>
                </configuration>
                <goals>
                  <goal>copy-resources</goal>
                </goals>
              </execution>
              <execution>
                <id>copy-citrus-ssh-schemas</id>
                <phase>post-site</phase>
                <configuration>
                  <outputDirectory>${project.build.directory}/site/schema/ssh/config</outputDirectory>
                  <resources>
                    <resource>
                      <directory>modules/citrus-model/model-ssh/src/main/resources/com/consol/citrus/schema</directory>
                      <includes>
                        <include>citrus-ssh-config*.xsd</include>
                      </includes>
                    </resource>
                  </resources>
                </configuration>
                <goals>
                  <goal>copy-resources</goal>
                </goals>
              </execution>
              <execution>
                <id>copy-citrus-mail-schemas</id>
                <phase>post-site</phase>
                <configuration>
                  <outputDirectory>${project.build.directory}/site/schema/mail/config</outputDirectory>
                  <resources>
                    <resource>
                      <directory>modules/citrus-model/model-mail/src/main/resources/com/consol/citrus/schema</directory>
                      <includes>
                        <include>citrus-mail-config*.xsd</include>
                      </includes>
                    </resource>
                  </resources>
                </configuration>
                <goals>
                  <goal>copy-resources</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
  
      <reporting>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-changes-plugin</artifactId>
            <version>2.6</version>
            <reportSets>
              <reportSet>
                <reports>
                  <report>changes-report</report>
                </reports>
              </reportSet>
            </reportSets>
          </plugin>
          <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>taglist-maven-plugin</artifactId>
            <version>2.4</version>
            <configuration>
              <tags>
                <tag>TODO</tag>
                <tag>@todo</tag>
                <tag>FIXME</tag>
                <tag>@deprecated</tag>
              </tags>
            </configuration>
          </plugin>
          <plugin>
            <artifactId>maven-javadoc-plugin</artifactId>
            <version>2.7</version>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-project-info-reports-plugin</artifactId>
            <version>2.4</version>
            <configuration>
              <dependencyDetailsEnabled>false</dependencyDetailsEnabled>
              <dependencyLocationsEnabled>false</dependencyLocationsEnabled>
            </configuration>
            <reportSets>
              <reportSet>
                <reports>
                  <report>dependency-convergence</report>
                  <report>dependency-management</report>
                  <report>project-team</report>
                </reports>
              </reportSet>
            </reportSets>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-plugin-plugin</artifactId>
            <version>2.7</version>
          </plugin>
        </plugins>
      </reporting>
    </profile>
    
    <profile>
      <id>assembly</id>
      <build>
        <plugins>
          <plugin>
            <groupId>com.agilejava.docbkx</groupId>
            <artifactId>docbkx-maven-plugin</artifactId>
            <executions>
              <execution>
                <id>html-reference-doc</id>
                <configuration>
                  <postProcess>
                    <copy todir="${project.basedir}/target/doc">
                      <fileset dir="${project.basedir}/target/docbkx">
                        <include name="**/*.html" />
                      </fileset>
                    </copy>
                    <copy todir="${project.basedir}/target/doc/html">
                      <fileset dir="${project.basedir}/src/docbkx">
                        <include name="**/*.css" />
                        <include name="**/*.png" />
                        <include name="**/*.gif" />
                        <include name="**/*.jpg" />
                      </fileset>
                    </copy>
                  </postProcess>
                </configuration>
                <goals>
                  <goal>generate-html</goal>
                </goals>
                <phase>process-resources</phase>
              </execution>
              <execution>
                <id>pdf-reference-doc</id>
                <configuration>
                  <postProcess>
                    <copy file="${project.basedir}/target/docbkx/pdf/index.pdf" 
                          tofile="${project.basedir}/target/doc/pdf/citrus-reference-${project.version}.pdf" failonerror="true" />
                  </postProcess>                    
                </configuration>
                <goals>
                  <goal>generate-pdf</goal>
                </goals>
                <phase>process-resources</phase>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <artifactId>maven-assembly-plugin</artifactId>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/dist-sources.xml</descriptor>
                <descriptor>src/main/assembly/dist-release.xml</descriptor>
              </descriptors>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>

    <profile>
      <id>assembly-antlibs</id>
      <build>
        <plugins>
          <plugin>
            <artifactId>maven-assembly-plugin</artifactId>
            <configuration>
              <descriptors>
                <descriptor>src/main/assembly/dist-antlibs.xml</descriptor>
              </descriptors>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
    
    <profile>
      <id>assembly-bundle</id>
      <build>
        <plugins>
          <plugin>
            <artifactId>maven-assembly-plugin</artifactId>
            <configuration>
              <descriptorRefs>
                <descriptorRef>jar-with-dependencies</descriptorRef>
              </descriptorRefs>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
  </profiles>
</project>
