                        <Export-Package>
                            org.milyn.*;-split-package:=merge-first;version="${project.version}",
                            META-INF.xsd.smooks.*;-split-package:=merge-first,
                            !META-INF.services.*,
                            !META-INF.maven.*
                        </Export-Package>
                        <Import-Package>
                            org.osgi.framework,
                            org.jaxen.*;version="1.1.1",
