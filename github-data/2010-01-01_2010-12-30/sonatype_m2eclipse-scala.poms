    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
		  <groupId>org.scala-tools.testing</groupId> 
		  <artifactId>specs_2.8.0.Beta1</artifactId> 
		  <version>1.6.4</version> 
      <scope>test</scope>
