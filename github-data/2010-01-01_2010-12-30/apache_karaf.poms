                                    <filtering>true</filtering>
                                    <includes>
                                        <include>**/*.properties</include>
                                        <include>**/*.cfg</include>
                                        <include>**/*.xml</include>
                                        <include>**/*.info</include>
                                    </includes>
                                </resource>
                                <resource>
                                	<directory>${project.basedir}/../assemblies/features/framework/src/main/snapshot</directory>
                                	<filtering>true</filtering>
