                                <include>org.fressian:fressian</include>
                            </includes>
                            <rules>
                                <rule>
                                    <pattern>org.objectweb.asm.**</pattern>
                                    <result>com.jitlogic.zorka.deps.asm.@1</result>
                                </rule>
                                <rule>
                                    <pattern>bsh.**</pattern>
                                    <result>com.jitlogic.zorka.deps.bsh.@1</result>
                                </rule>
                                <rule>
                                    <pattern>com.jitlogic.contrib.**</pattern>
                                    <result>com.jitlogic.zorka.deps.@1</result>
                                </rule>
                                <rule>
                                    <pattern>org.fressian.**</pattern>
                                    <result>com.jitlogic.zorka.deps.fressian.@1</result>
                                </rule>
                                <keep>
                                    <pattern>com.jitlogic.zorka.**</pattern>
                                </keep>
                            </rules>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>${surefire.plugin.version}</version>
                <configuration>
                    <includes>
                        <include>**/*UnitTest.java</include>
                    </includes>
                </configuration>
            </plugin>
        </plugins>
    </build>

