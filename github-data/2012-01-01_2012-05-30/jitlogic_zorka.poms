<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.jitlogic.zorka</groupId>
    <artifactId>zorka</artifactId>
    <packaging>pom</packaging>
    <version>1.0.1</version>
    <name>zorka: parent aggregator</name>
    <url>http://zorka.io</url>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

        <antrun.plugin.version>1.3</antrun.plugin.version>
        <compiler.plugin.version>2.3.2</compiler.plugin.version>
        <dependency.plugin.version>2.3</dependency.plugin.version>
        <jar.plugin.version>2.4</jar.plugin.version>
        <jarjar.plugin.version>1.7</jarjar.plugin.version>
        <source.plugin.version>2.1.2</source.plugin.version>
        <surefire.plugin.version>2.12</surefire.plugin.version>
        <war.plugin.version>2.1.1</war.plugin.version>

        <asm.version>4.2</asm.version>
        <bval.version>0.5</bval.version>
        <dbcp.version>1.4</dbcp.version>
        <fest.version>1.4</fest.version>
        <fressian.version>0.6.3</fressian.version>
        <gin.version>2.1.0</gin.version>
        <guava.version>15.0</guava.version>
        <guice.version>3.0</guice.version>
        <gwt.version>2.5.1</gwt.version>
        <gxt.version>3.0.1</gxt.version>
        <jetty.version>6.1.26</jetty.version>
        <json.version>1.2</json.version>
        <junit.version>4.11</junit.version>
        <libsnmp.version>0.0.1</libsnmp.version>
        <logback.version>1.0.13</logback.version>
        <mapdb.version>0.9.8</mapdb.version>
        <parboiled.version>1.1.6</parboiled.version>
        <slf4j.version>1.7.5</slf4j.version>
        <zoola.version>0.1.0</zoola.version>
    </properties>

    <repositories>
        <repository>
            <id>zorka-repo</id>
            <name>Artifacts for Zorka agent</name>
            <url>http://zorka.io/repo</url>
        </repository>
    </repositories>

    <modules>
        <module>zorka-common</module>
        <module>zorka-common-test</module>
        <module>zorka-core</module>
        <module>zorka-agent</module>
        <module>zorka-viewer</module>
        <module>zorka-dist</module>
        <module>zico-core</module>
        <module>zico-util</module>
        <module>zico-web</module>
        <module>zico-dist</module>
    </modules>

</project>
