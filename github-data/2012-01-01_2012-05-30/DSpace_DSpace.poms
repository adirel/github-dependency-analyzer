            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>sword</module>
            </modules>
        </profile>
        <profile>
            <id>dspace-swordv2</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>swordv2</module>
            </modules>
        </profile>
        <profile>
            <id>dspace-solr</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>solr</module>
            </modules>
        </profile>
        <profile>
            <id>dspace-oai</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>oai</module>
            </modules>
        </profile>
