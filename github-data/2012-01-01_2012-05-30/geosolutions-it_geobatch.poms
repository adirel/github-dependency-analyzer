      </modules>
    </profile>
    <profile>
      <id>task-executor</id>
      <activation>
        <property>
          <name>all</name>
          <value>true</value>
        </property>
      </activation>
      <modules>
        <module>geoserver</module>
        <module>task-executor</module>
      </modules>
    </profile>
    <profile>
      <id>scripting</id>
      <activation>
