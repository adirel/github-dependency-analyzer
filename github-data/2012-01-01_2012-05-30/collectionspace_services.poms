							<goals>
								<goal>generate</goal>
							</goals>
							<configuration>
								<extension>true</extension>
								<bindingIncludes>
                					<include>xbindings.xjb</include>
              					</bindingIncludes>
								<args>
								 	<arg>-Xannotate</arg> 
