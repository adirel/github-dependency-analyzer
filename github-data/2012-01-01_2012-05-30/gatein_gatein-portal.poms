                 <goals> <goal>stop</goal> </goals> </execution>
              -->
            </executions>
            <configuration>
              <wait>false</wait>
              <container>
                <containerId>${containerId}</containerId>
                <append>false</append>
                <home>${container.home}</home>
                <timeout>${cargo.timeout}</timeout>
