   <name>webservice-example EAR module</name>

   <dependencies>

      <dependency>
         <groupId>de.akquinet.jbosscc</groupId>
         <artifactId>webservice-example-web</artifactId>
         <type>war</type>
      </dependency>

      <dependency>
         <groupId>de.akquinet.jbosscc</groupId>
         <artifactId>webservice-example-ejbclient</artifactId>
         <type>ejb</type>
