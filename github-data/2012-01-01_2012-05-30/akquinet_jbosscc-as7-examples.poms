      <dependency>
         <groupId>de.akquinet.jbosscc</groupId>
         <artifactId>web-application-example-web</artifactId>
         <type>war</type>
      </dependency>
      <dependency>
         <groupId>de.akquinet.jbosscc</groupId>
         <artifactId>web-application-example-ejb</artifactId>
