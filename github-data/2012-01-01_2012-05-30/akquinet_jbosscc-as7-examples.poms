      <finalName>web-application-example</finalName>
      <plugins>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-ear-plugin</artifactId>
            <version>2.6</version>
            <configuration>
               <version>6</version>
               <defaultLibBundleDir>lib</defaultLibBundleDir>
               <modules>

                  <webModule>
                     <groupId>de.akquinet.jbosscc</groupId>
                     <artifactId>web-application-example-web</artifactId>
