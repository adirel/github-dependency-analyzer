    <groupId>org.dbpedia.spotlight</groupId>
    <artifactId>spotlight</artifactId>
    <version>0.7</version>
    <packaging>pom</packaging>
    <name>DBpedia Spotlight annotation framework (parent POM)</name>

    <modules>
        <module>core</module>
        <module>rest</module>
