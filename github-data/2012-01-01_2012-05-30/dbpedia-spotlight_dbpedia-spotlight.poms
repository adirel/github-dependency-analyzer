                            <jvmArgs>
                                <jvmArg>-Xmx${javaOpts.Xmx}</jvmArg>
                            </jvmArgs>
                            <!--args>
                                <arg>../conf/indexing-3.8.properties</arg>
                                <arg>output/occs.tsv</arg>
                            </args-->
                        </launcher>
                        <launcher>
                            <id>IndexLingPipeSpotter</id>
                            <mainClass>org.dbpedia.spotlight.spot.lingpipe.IndexLingPipeSpotter</mainClass>
                            <jvmArgs>
                                <jvmArg>-Xmx${javaOpts.Xmx}</jvmArg>
                            </jvmArgs>
