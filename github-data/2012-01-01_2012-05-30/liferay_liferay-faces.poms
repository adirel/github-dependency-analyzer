	<name>Liferay Faces Test Issues Portal</name>
	<packaging>pom</packaging>

	<modules>
		<module>FACES-257-portlet</module>
		<!-- The following portlets utilize liferay-ui:input-editor and cannot be tested on WebLogic due to FACES-1602 -->
