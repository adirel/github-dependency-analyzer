                        <id>jasmine-tests</id>
                        <goals>
                            <goal>test</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <preloadSources>
                        <include>assets/common/vendor/jquery.js</include>
                        <include>assets/common/vendor/jquery-ui-1.9.0.custom.min.js</include>
                        <include>assets/common/vendor/angular/1.1.5/angular.min.js</include>
