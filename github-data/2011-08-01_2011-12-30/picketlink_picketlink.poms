      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>org.picketlink.forge</groupId>
      <artifactId>picketlink-forge-spi</artifactId>
      <classifier>forge-addon</classifier>
    </dependency>
    <dependency>
      <groupId>org.jboss.forge.addon</groupId>
      <artifactId>projects</artifactId>
      <classifier>forge-addon</classifier>
    </dependency>
