<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.phenotips</groupId>
    <artifactId>phenotips-parent</artifactId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <artifactId>phenotips-distribution</artifactId>
  <packaging>pom</packaging>
  <name>PhenoTips - Distribution packages</name>

  <properties>
    <!-- Distribution packages have no Java code, speedup by skipping Clirr and Checkstyle checks -->
    <checkstyle.skip>true</checkstyle.skip>
    <clirr.skip>true</clirr.skip>
  </properties>

  <modules>
    <module>hsqldb-database</module>
    <module>war</module>
    <module>solr-war</module>
    <module>standalone</module>
  </modules>

  <profiles>
    <profile>
      <id>rpm</id>
      <activation>
        <activeByDefault>false</activeByDefault>
        <file>
          <exists>/usr/bin/rpmbuild</exists>
        </file>
      </activation>
      <modules>
        <module>rpm</module>
      </modules>
    </profile>
  </profiles>
</project>

