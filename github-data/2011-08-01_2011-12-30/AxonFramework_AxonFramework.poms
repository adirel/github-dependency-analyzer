    <description>
        Module containing some examples for several building blocks available in Axon 2.
    </description>

    <dependencies>
        <!-- Axon dependencies -->
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>axon-core</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
