                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-resources-plugin</artifactId>
                    <version>2.6</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-scm-plugin</artifactId>
                    <version>1.7</version>
                </plugin>
	            <plugin>
	                <groupId>org.codehaus.mojo</groupId>
	                <artifactId>buildnumber-maven-plugin</artifactId>
	                <version>1.1</version>
                </plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-javadoc-plugin</artifactId>
					<version>2.9</version>
					<configuration>
	                    <archive>
	                        <!-- Extra attributes added to the MANIFEST.MF file -->
	                        <manifestEntries>
	                            <!-- see Package constructor using Manifest -->
	                            <Implementation-Title>${project.artifactId}</Implementation-Title>
	                            <Implementation-Version>${project.version}</Implementation-Version>
	                            <Implementation-Vendor>Insight</Implementation-Vendor>
	                               <!-- NOTE: this name must match the one used in InsightVersion class -->
	                            <Release-Timestamp>${buildtimestamp}</Release-Timestamp>
	                        </manifestEntries>
	                    </archive>
						<quiet>true</quiet>
						<links>
							<link>http://docs.mockito.googlecode.com/hg/${mockito.version}</link>
							<link>http://kentbeck.github.com/junit/javadoc/${junit.version}</link>
						</links>
					</configuration>
				</plugin>
            </plugins>
        </pluginManagement>
        <plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
				<version>1.1.1</version>
				<executions>
					<execution>
						<id>enforce-versions</id>
						<goals>
							<goal>enforce</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<rules>
						<requireMavenVersion>
							<version>[3.0,)</version>
						</requireMavenVersion>
					</rules>
				</configuration>
			</plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <version>1.8</version>
                <executions>
                    <execution>
                        <id>maven-version</id>
                        <phase>initialize</phase>
                        <goals>
                          <goal>maven-version</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.groovy.maven</groupId>
                <artifactId>gmaven-plugin</artifactId>
                <version>${gmaven.plugin.version}</version>
                <executions>
                    <!-- Works only for Maven 3.x -->
					<execution>
                        <id>detect-insight-root-dir</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
							<![CDATA[
                                for (java.io.File file=new java.io.File(new java.net.URI('${project.baseUri}')); file != null; file = file.getParentFile()) {
                                    if (!file.isDirectory()) {
                                        continue
                                    }

                                    java.io.File    testFile=new java.io.File(file, '.gitmodules')
                                    if (testFile.canRead()) {
                                        project.properties['insight.root.dir'] = file.getAbsolutePath()
										return
                                    }
                                }

                                System.err.println("Failed to detected Insight root dir")
							]]>
                            </source>
                        </configuration>
                    </execution>
                    <execution>
                        <id>generate-timestamp-property</id>
                        <phase>validate</phase>
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
                            <source>
							<![CDATA[
                                project.properties['buildtimestamp'] = java.text.MessageFormat.format("{0,date,yyyyMMddHHmmss}", new Date())
							]]>
                            </source>
                        </configuration>
                    </execution>
                    <execution>
                        <id>resolve-distribution-details</id>
                        <phase>install</phase>	<!-- A pre-deploy phase - had it existed - would have been better -->
                        <goals>
                            <goal>execute</goal>
                        </goals>
                        <configuration>
								<!-- see http://maven.apache.org/ref/3.0.4/maven-core/apidocs/org/apache/maven/execution/MavenSession.html -->
                            <source>
							<![CDATA[
								if (!session.goals.contains("deploy")) {
									return;
								}

                                if ('${project.version}'.contains("SNAPSHOT")) {
                                    session.currentProject.distributionManagement.snapshotRepository.id = project.properties['snapshot.repo.id']
                                    session.currentProject.distributionManagement.snapshotRepository.name = project.properties['snapshot.repo.name']
                                    session.currentProject.distributionManagement.snapshotRepository.url = project.properties['snapshot.dist.url']

									project.properties['deploy.repo.id'] = project.properties['snapshot.repo.id']
									project.properties['deploy.repo.name'] = project.properties['snapshot.repo.name']
									project.properties['deploy.repo.url'] = project.properties['snapshot.dist.url']
                                } else if ('${project.version}'.contains("RELEASE") || '${project.version}'.contains(".SR")) {
                                    session.currentProject.distributionManagement.repository.id = project.properties['release.repo.id']
                                    session.currentProject.distributionManagement.repository.name = project.properties['release.repo.name']
                                    session.currentProject.distributionManagement.repository.url = project.properties['release.dist.url']

									project.properties['deploy.repo.id'] = project.properties['release.repo.id']
									project.properties['deploy.repo.name'] = project.properties['release.repo.name']
									project.properties['deploy.repo.url'] = project.properties['release.dist.url']
                                } else {
                                    session.currentProject.distributionManagement.repository.id = project.properties['milestone.repo.id']
                                    session.currentProject.distributionManagement.repository.name = project.properties['milestone.repo.name']
                                    session.currentProject.distributionManagement.repository.url = project.properties['milestone.dist.url']

									project.properties['deploy.repo.id'] = project.properties['milestone.repo.id']
									project.properties['deploy.repo.name'] = project.properties['milestone.repo.name']
									project.properties['deploy.repo.url'] = project.properties['milestone.dist.url']
                                }
							]]>
                            </source>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>2.1.2</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<phase>verify</phase>
						<goals>
							<goal>jar-no-fork</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.5.1</version>
                <configuration>
                    <source>${java.source.version}</source>
                    <target>${java.target.version}</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.6</version>
                <executions>
                    <execution>
                        <id>install</id>
                        <phase>install</phase>
                        <goals>
                            <goal>sources</goal>
                        </goals>
                        <configuration>
                            <markersDirectory>${settings.localRepository}/com/springsource/insight/dependency-maven-plugin-markers</markersDirectory>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>2.4</version>
                <configuration>
                    <archive>
                        <addMavenDescriptor>true</addMavenDescriptor>
						<!-- Generate index file for quicker access -->
						<index>true</index>
						<!-- Extra attributes added to the MANIFEST.MF file -->
						<manifestEntries>
							<!-- see Package constructor using Manifest -->
							<Implementation-Title>${project.artifactId}</Implementation-Title>
							<Implementation-Version>${project.version}</Implementation-Version>
							<Implementation-Vendor>Insight</Implementation-Vendor>
							<Release-Timestamp>${buildtimestamp}</Release-Timestamp>
                            <Maven-Version>${maven.version}</Maven-Version>
						</manifestEntries>
                    </archive>
                    <excludes>
                        <exclude>**/.gitignore</exclude>
                        <exclude>**/builddef.*</exclude>
                    </excludes>
                </configuration>
            </plugin>
            <plugin>
              <groupId>org.apache.maven.plugins</groupId>
              <artifactId>maven-surefire-plugin</artifactId>
              <version>${surefire.plugin.version}</version>
              <configuration>
					<runOrder>alphabetical</runOrder>
					<includes>
						<include>**/*Test.java</include>
					</includes>
					<excludedGroups>${excluded.tests.group}</excludedGroups>
                  <forkMode>once</forkMode><!-- at least once -->
                        <!-- avoid inherent multi-threaded tests -->
                  <perCoreThreadCount>false</perCoreThreadCount>
                  <threadCount>1</threadCount>
				  <systemPropertyVariables>
						<java.io.tmpdir>${project.build.directory}${file.separator}temp</java.io.tmpdir>
						<java.awt.headless>true</java.awt.headless>
				  </systemPropertyVariables>
              </configuration>
					<!-- need to include it to ensure grouping detection -->
			  <dependencies>
				 <dependency>
					<groupId>org.apache.maven.surefire</groupId>
					<artifactId>surefire-junit47</artifactId>
					<version>${surefire.plugin.version}</version>
				 </dependency>
			  </dependencies>
            </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
                <version>2.7</version>
					<!-- NOTE: by default we do not deploy anything not compiled in 1.5
					   - see special profile activate for 1.5 -->
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
        </plugins>
    </build>

    <reporting>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-project-info-reports-plugin</artifactId>
                <version>2.4</version>
                <reportSets>
                    <reportSet>
                        <reports>
                        </reports>
                    </reportSet>
                </reportSets>
            </plugin>
        </plugins>
    </reporting>

    <scm>
        <connection>scm:git:ssh://git@github.com/SpringSource/spring-insight-plugins.git</connection>
        <url>scm:git:ssh://git@github.com/SpringSource/spring-insight-plugins.git</url>
        <developerConnection>scm:git:ssh://git@github.com/SpringSource/spring-insight-plugins.git</developerConnection>
    </scm>


        <!-- Note: these values are placeholders - the real values are
            set by a gmaven-plugin execution bound to the 'install' phase
            provided a 'deploy' goal has been specified in the Maven
            invocation commmand -->
    <distributionManagement>
        <repository>
            <id>insight-releases</id>
            <name>Spring Insight Releases Repository</name>
            <url>http://localhost:8081/artifactory/libs-releases-local</url>
        </repository>
        <snapshotRepository>
            <id>insight-snapshots</id>
            <name>Spring Insight Snapshots Repository</name>
            <url>http://localhost:8081/artifactory/libs-snapshots-local</url>
        </snapshotRepository>
    </distributionManagement>
</project>
