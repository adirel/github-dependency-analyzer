						</Bundle-Activator>
						<Private-Package>
							org.bridj.relocated.*
						</Private-Package>
						<Import-Package>
							org.objectweb.asm;resolution:=optional,
							android.app;resolution:=optional,
							android.content.pm;resolution:=optional,
							android.os;resolution:=optional,
							com.android.dx.dex;resolution:=optional,
