                      <querydsl.entityAccessors>true</querydsl.entityAccessors>
                      <querydsl.listAccessors>true</querydsl.listAccessors>
                      <querydsl.mapAccessors>true</querydsl.mapAccessors>
                    </options>
                  </configuration>
                </execution>
              </executions>
