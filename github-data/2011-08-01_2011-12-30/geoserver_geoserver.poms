						<addSvnKeyWords>true</addSvnKeyWords>


					</configuration>
					<executions>
						<execution>
							<id>first</id>
							<goals>
								<goal>update-file-header</goal>
							</goals>
							<phase>process-sources</phase>
							<configuration>
								<licenseName>gpl_v2</licenseName>
								<roots>
									<root>${basedir}/geoserver-sync-core/</root>
