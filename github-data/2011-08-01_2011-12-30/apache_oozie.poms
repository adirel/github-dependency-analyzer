                                <Xlint/>
                                <Xmaxwarns>9999</Xmaxwarns>
                            </compilerArguments>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>hadoop-23</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <properties>
               <hadoop.version>0.23.5</hadoop.version>
