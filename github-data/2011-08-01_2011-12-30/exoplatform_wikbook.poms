                <configuration>
                  <artifacts>
                    <artifact>
                      <file>target/wikbook/output/pdf/index.pdf</file>
                      <type>pdf</type>
                    </artifact>
                  </artifacts>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>

  </profiles>

