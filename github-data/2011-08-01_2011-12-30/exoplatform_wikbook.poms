                  <targetDirectory>${project.build.directory}/wikbook/output/html</targetDirectory>
                  <htmlCustomization>xsl/bootstrap/html.xsl</htmlCustomization>
                </configuration>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <artifactId>maven-assembly-plugin</artifactId>
