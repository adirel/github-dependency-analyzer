		<log4j.version>1.2.17</log4j.version>
		<junit.version>4.10</junit.version>
		<openjpa.version>2.2.0</openjpa.version>
		<eclipselink.version>2.3.2</eclipselink.version>
		<hibernate.version>4.0.1.Final</hibernate.version>
