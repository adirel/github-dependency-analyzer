        </license>
    </licenses>
    <build>
        <plugins>
            <plugin>
                <inherited>true</inherited>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-enforcer-plugin</artifactId>
                <version>1.0-beta-1</version>
                <executions>
                    <execution>
                        <id>enforce-property</id>
                        <phase>validate</phase>
                        <goals>
                            <goal>enforce</goal>
                        </goals>
                        <configuration>
                            <rules>
                                <requireProperty>
                                    <property>bpa</property>
                                    <message>"You must specify a platform and architecture!"</message>
                                </requireProperty>
                            </rules>
                            <fail>true</fail>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <inherited>true</inherited>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>unpack-test-dependencies</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>unpack-dependencies</goal>
                        </goals>
                        <configuration>
                            <excludeScope>runtime</excludeScope>
                            <includes>**/*</includes>
                            <markersDirectory>${project.build.directory}/cxx/test-markers</markersDirectory>
                            <outputDirectory>${project.build.directory}/cxx/test</outputDirectory>
                            <overWriteReleases>false</overWriteReleases>
                            <overWriteIfNewer>false</overWriteIfNewer>
                            <overWriteSnapshots>true</overWriteSnapshots>
                            <excludeTypes>${exclude_unpack_type}</excludeTypes>
                        </configuration>
                    </execution>
                    <execution>
                        <id>unpack-runtime-dependencies</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>unpack-dependencies</goal>
                        </goals>
                        <configuration>
                            <includeScope>runtime</includeScope>
                            <excludes>include/**</excludes>
                            <markersDirectory>${project.build.directory}/cxx/runtime-markers</markersDirectory>
                            <outputDirectory>${project.build.directory}/cxx/runtime</outputDirectory>
                            <overWriteReleases>false</overWriteReleases>
                            <overWriteIfNewer>false</overWriteIfNewer>
                            <overWriteSnapshots>true</overWriteSnapshots>
                            <excludeTypes>${exclude_unpack_type}</excludeTypes>
                        </configuration>
                    </execution>
                    <execution>
                        <id>unpack-compile-dependencies</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>unpack-dependencies</goal>
                        </goals>
                        <configuration>
                            <includeScope>compile</includeScope>
                            <includes>**/*</includes>
                            <markersDirectory>${project.build.directory}/cxx/compile-markers</markersDirectory>
                            <outputDirectory>${project.build.directory}/cxx/compile</outputDirectory>
                            <overWriteReleases>false</overWriteReleases>
                            <overWriteIfNewer>false</overWriteIfNewer>
                            <overWriteSnapshots>true</overWriteSnapshots>
                            <excludeTypes>${exclude_unpack_type}</excludeTypes>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <inherited>true</inherited>
                <groupId>org.jboss.narayana.blacktie.plugins</groupId>
                <artifactId>blacktie-cpp-plugin</artifactId>
                <version>${project.version}</version>
                <configuration>
                    <configuration.type>${configuration.type}</configuration.type>
                    <maven.test.skip>${maven.test.skip}</maven.test.skip>
                    <skipTests>${skipTests}</skipTests>
                    <src.main>${src.main}</src.main>
                    <failOnErrorTests>${failOnErrorTests}</failOnErrorTests>
                    <generate.testrunner>${generate.testrunner}</generate.testrunner>
                    <export.dll>${export.dll}</export.dll>
                    <blacktie.configuration.dir>${blacktie.configuration.dir}</blacktie.configuration.dir>
                    <jbossas.ip.addr>${jbossas.ip.addr}</jbossas.ip.addr>
                    <test.includes>${test.includes}</test.includes>
                    <test.excludes>${test.excludes}</test.excludes>
                    <use.valgrind>${use.valgrind}</use.valgrind>
                </configuration>
                <executions>
                    <execution>
                        <id>generate-sources</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>generate-idl-sources</goal>
                            <goal>generate-proto-sources</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>compile</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test-compile</id>
                        <phase>test-compile</phase>
                        <goals>
                            <goal>test-compile</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test</id>
                        <phase>test</phase>
                        <goals>
                            <goal>test</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>process-classes</id>
                        <phase>process-classes</phase>
                        <goals>
                            <goal>process-classes</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-jar-plugin</artifactId>
                <executions>
                    <execution>
                        <id>jar-without-classifier</id>
                        <phase>package</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                        <configuration>
                            <excludes>
                                <exclude>**</exclude>
                            </excludes>
                            <forceCreation>true</forceCreation>
                        </configuration>
                    </execution>
                    <execution>
                        <id>jar-with-classifier</id>
                        <phase>package</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                        <configuration>
                            <classifier>${bpa}</classifier>
                            <forceCreation>true</forceCreation>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        <pluginManagement>
        	<plugins>
        		<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
        		<plugin>
        			<groupId>org.eclipse.m2e</groupId>
        			<artifactId>lifecycle-mapping</artifactId>
        			<version>1.0.0</version>
        			<configuration>
        				<lifecycleMappingMetadata>
        					<pluginExecutions>
        						<pluginExecution>
        							<pluginExecutionFilter>
