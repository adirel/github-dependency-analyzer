        						</pluginExecution>
        						<pluginExecution>
        							<pluginExecutionFilter>
        								<groupId>
        									org.jboss.narayana.blacktie.plugins
        								</groupId>
        								<artifactId>
        									blacktie-cpp-plugin
        								</artifactId>
        								<versionRange>
        									[5.0.2.Final-SNAPSHOT,)
        								</versionRange>
        								<goals>
        									<goal>test-compile</goal>
        									<goal>
        										generate-idl-sources
        									</goal>
        									<goal>compile</goal>
        									<goal>
        										generate-proto-sources
        									</goal>
        									<goal>
        										generate-test-lib
        									</goal>
        									<goal>process-classes</goal>
        								</goals>
        							</pluginExecutionFilter>
        							<action>
        								<ignore></ignore>
        							</action>
        						</pluginExecution>
